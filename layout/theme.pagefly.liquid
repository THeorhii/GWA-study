        <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.css">
    {% endif %}
  
    <script>
        document.documentElement.className = document.documentElement.className.replace('no-js', 'js');
        window.theme = {
            strings: {
                addToCart: {{ 'products.product.add_to_cart' | t | json }},
                soldOut: {{ 'products.product.sold_out' | t | json }},
                unavailable: {{ 'products.product.unavailable' | t | json }},
            },
            moneyFormat: {{ shop.money_format | json }},
            kitchenCentreKey: '{{ shop.metafields.kitchen.centre }}',
            cartType: {{ settings.cart_action | json }},
            minOrderSum: {{ settings.cart_min_order_sum | json }},
            bannersCode: [
                ['{{ settings.cart_drawer_city_name_2 | downcase }}', '{{ settings.cart_drawer_city_codes_2 }}'],
                ['{{ settings.cart_drawer_city_name_3 | downcase }}', '{{ settings.cart_drawer_city_codes_3 }}'],
                ['{{ settings.cart_drawer_city_name_4 | downcase }}', '{{ settings.cart_drawer_city_codes_4 }}'],
                ['{{ settings.cart_drawer_city_name_5 | downcase }}', '{{ settings.cart_drawer_city_codes_5 }}'],
                ['{{ settings.cart_drawer_city_name_6 | downcase }}', '{{ settings.cart_drawer_city_codes_6 }}'],
                ['{{ settings.cart_drawer_city_name_7 | downcase }}', '{{ settings.cart_drawer_city_codes_7 }}'],
                ['{{ settings.cart_drawer_city_name_8 | downcase }}', '{{ settings.cart_drawer_city_codes_8 }}'],
                ['{{ settings.cart_drawer_city_name_9 | downcase }}', '{{ settings.cart_drawer_city_codes_9 }}'],
                ['{{ settings.cart_drawer_city_name_10 | downcase }}', '{{ settings.cart_drawer_city_codes_10 }}'],
                ['{{ settings.cart_drawer_city_name_11 | downcase }}', '{{ settings.cart_drawer_city_codes_11 }}']
            ]
        };

    </script>

    {% if template.directory == 'customers' %}
