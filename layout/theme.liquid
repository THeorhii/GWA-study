                ['{{ settings.cart_drawer_city_name_10 | downcase }}', '{{ settings.cart_drawer_city_codes_10 }}'],
                ['{{ settings.cart_drawer_city_name_11 | downcase }}', '{{ settings.cart_drawer_city_codes_11 }}']
