<body id="{{ page_title | handle }}" class="template-{{ template.name | handle }} template-{{ template | handle }} {% if template.suffix == "default" %}simple-page{% endif %}"
    data-customer="{{ customer.id }}">
    <script async src="https://t.cfjump.com/tag/74758"></script>

    <a class="in-page-link visually-hidden skip-link" href="#MainContent">{{ 'general.accessibility.skip_to_content' | t }}</a>

    {% section 'header' %}

    {% include 'cart-drawer' %}

    {% include 'cart-drawer--buy-more' %}

    <main role="main" id="MainContent">
        {{ content_for_layout }}
    </main>

    {% if settings.cart_action == 'drawer' %}
        {% include 'ajax-cart-template' %}
    {% endif %}

    {% section 'footer' %}

    <script defer="defer">
        $(document).ready(function() {
            /* Meals data from App*/
            let key_kitchen = window.theme.kitchenCentreKey;
            $.getJSON('/apps/' + key_kitchen + '/getMealsFilter', function (data) {
                if (data) {
                {% if template.name == 'collection' or template.name == 'product' %}
                    {% if template.suffix contains 'meal-pack' %}
                    theme.mealPackProducts.mealPackChangeProduct(data.products);
                    {% endif %}
                {% endif %}

                    productPopUp(data.products);
                    slate.subscriptions.changeMealsPage(data.products, key_kitchen);
                    theme.mealPackProducts.mealPackAddToCartProducts();
                }
            });
            /* Regions data from App*/
            $.getJSON('/apps/' + key_kitchen + '/getDeliveryRegions', function (data) {
                if (data) {
                    slate.subscriptions.singleSubscription(data, key_kitchen);
                    if ($('body').hasClass('delivery-information')) {
                        mainDeliveryMap(data);
                    }

                    if (window.theme.cartType == 'drawer') {
                        cartDrawerDelivery(data);
                        forCheckSavedInfo(data);
                        drawerButtonCheckout(data);
                    } else {
                        slate.cart.cartDelivery(data);
                    }
                }
            });
        })
    </script>

    <script>
        /* Meal Packs Data */
        window.mealPacksDetails = window.mealPacksDetails || {};
        {% assign meal_packs_collection = collections[settings.meal_packs_collection] %}
        {% paginate meal_packs_collection.products by 500 %}
        {% for product in meal_packs_collection.products %}
            {% assign meal_pack_thumb = product.featured_image | img_url: '150x150' %}
            {% for image in product.images %}
                {% if image.alt contains 'featured-image' %}
                    {% assign meal_pack_thumb = image | img_url: '150x150' %}
                    {% break %}
                {% endif %}
            {% else %}
                {% assign meal_pack_thumb = settings.meal_pack_default_image | img_url: '150x150' %}
            {% endfor %}
            (function(){
                mealPacksDetails[{{ product.id }}] = {
                    title: "{{ product.title }}",
                    url: "{{ product.url }}",
                    thumb: "{{ meal_pack_thumb }}"
                };
            })();
        {% endfor %}
        {% endpaginate %}
    </script>

    <script>
        _mpevt.push(['WorkoutMeals._event', 'WebsiteVisitor', '1']);
    </script>

    <script type="text/javascript">
        var _support = _support || { 'ui': {}, 'user': {} };
        _support['account'] = 'WorkoutMeals';
    </script>

 {% include 'smile-initializer' %}

{% include 'social-login' %}
</body>
