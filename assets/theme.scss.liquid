/*===============================================
* Styles for blog
This stylesheet use:
- blog (template)
- about-us (template)
- about-us--news (section)
- blog-simple-slider (section)
- blog-grid-item (snippet)
* ================================================*/

.blogs-container {
  margin-bottom: 104px;
}
.blogs-container .blogs-wrapper {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin: 56px auto 24px;
}
.blogs-container .blogs-wrapper .blog-content {
  display: flex;
  flex-direction: column;
  width: 31%;
  margin-bottom: 32px;
}
.blogs-container .blogs-wrapper .blog-content img {
  width: 100%;
  max-height: 240px;
  height: 200px;
  object-fit: cover;
  border-radius: 0 32px 0 0;
  border: solid 1px rgba(182, 182, 182, 0.5);
}
.blogs-container .blogs-wrapper .blog-content .blog-info {
  display: flex;
  flex-direction: column;
  padding: 24px 32px 32px;
  text-align: left;
  font-family: var(--font-stack-body);
  background-color: #fff;
  border-radius: 0 0 0 32px;
  transition: background-color 0.3s linear;
  border-bottom: solid 1px rgba(182, 182, 182, 0.5);
  border-left: solid 1px rgba(182, 182, 182, 0.5);
  border-right: solid 1px rgba(182, 182, 182, 0.5);
}
.blogs-container .blogs-wrapper .blog-content .blog-info h4 {
  margin-top: 0;
  margin-bottom: 8px;
  font: bold calc(var(--font-size-base) + 6px) var(--font-stack-header);
  letter-spacing: 1px;
  min-height: 62px;
}
.blogs-container .blogs-wrapper .blog-content .blog-info h4 a {
  color: var(--color-dark-text);
  text-decoration: none;
}
.blogs-container .blogs-wrapper .blog-content .blog-info li a {
  color: var(--color-dark-text);
  text-decoration: none;
}
.blogs-container .blogs-wrapper .blog-content .blog-info .info-about-post {
  margin: 0 0 16px;
  font-size: calc(var(--font-size-base) - 4px);
  font-weight: normal;
  line-height: 1.33;
  letter-spacing: normal;
  color: var(--color-yellow);
}
.blogs-container .blogs-wrapper .blog-content .blog-info .rte {
  flex-grow: 1;
  margin: 0 0 16px;
  font-size: var(--font-size-base);
  font-weight: normal;
  line-height: 1.5;
  letter-spacing: normal;
  color: var(--color-light-grey);
}
.blogs-container .blogs-wrapper .blog-content .blog-info .rte p {
  margin: 0;
}
.blogs-container .blogs-wrapper .blog-content .blog-info .btn-yellow {
  width: max-content;
  padding: 5px 25px;
  font: bold calc(var(--font-size-base) - 2px) var(--font-stack-header);
  line-height: normal;
  letter-spacing: 1px;
  text-align: center;
  color: var(--color-bright);
  background-color: var(--color-dark-text);
}
.blogs-container .blogs-wrapper .blog-content:not(:nth-child(3n)) {
  margin-right: 32px;
}
.blogs-container .blogs-wrapper .blog-content:hover .info-about-post {
  color: var(--color-dark-text);
}
.blogs-container .blogs-wrapper .blog-content:hover .blog-info {
  background-color: var(--color-bright);
}
.blogs-container .blogs-wrapper .blog-content:hover img {
  border-bottom-color: var(--color-bright);
}
.blogs-container .blogs-slider {
  max-width: 1680px;
  width: 100%;
  padding: 0 56px 32px;
}

.blogs-container .blogs-slider .slick-track {
  display: flex;
  flex-direction: row;
}
.blogs-container .blogs-slider .slick-track .slick-slide {
  display: flex;
  height: auto;
}
.blogs-container .blogs-slider .slide {
  padding: 0 16px;
}
.blogs-container .blogs-slider .slide .blog-content {
  width: auto;
}
.blogs-container .blogs-slider .slide .blog-content:not(:nth-child(2n)) {
  margin-right: 0;
}
.blogs-container .blogs-slider .slick-prev:before, .blogs-container .blogs-slider .slick-next:before {
  content: '';
  border: solid var(--color-light-grey);
  border-width: 0 3px 3px 0;
  display: inline-block;
  padding: 8px;
}
.blogs-container .blogs-slider .slick-prev:hover:before, .blogs-container .blogs-slider .slick-next:hover:before {
  border-color: var(--color-dark-text);
}
.blogs-container .blogs-slider .slick-prev {
  left: 32px;
}
.blogs-container .blogs-slider .slick-prev:before {
  transform: rotate(135deg);
  -webkit-transform: rotate(135deg);
}
.blogs-container .blogs-slider .slick-next {
  right: 32px;
}
.blogs-container .blogs-slider .slick-next:before {
  transform: rotate(-45deg);
  -webkit-transform: rotate(-45deg);
}
.blogs-container .pagination {
  display: block;
  margin: 0;
  text-align: center;
}
.blogs-container .pagination span {
  /*border: none !important;
   background-color: transparent;
  */
  border-color: var(--color-white);
  margin-right: 2px;
}
.blogs-container .pagination .prev, .blogs-container .pagination .next, .blogs-container .pagination .page {
  padding: 8px 18px;
  font: bold calc(var(--font-size-base) - 2px) var(--font-stack-header);
  line-height: normal;
  letter-spacing: 1px;
  text-align: center;
  color: var(--color-dark-text);
}
.blogs-container .pagination .prev a, .blogs-container .pagination .next a, .blogs-container .pagination .page a {
