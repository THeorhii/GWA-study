/*===============================================
* Banner Video & Slider Styles
* ================================================*/
.template-index{
  width: 100%;
  overflow-x: hidden;
}

.main-video-banner{
  position: relative;
  right: 0;
  transition: 1s;
  width: 100%;
  z-index: 0;
  &.hid{
    right: 100%;
  }
  .img-before-video{
    background-color: #010101;
    width: 100%;
    position: absolute;
    z-index: 1000;
    left: 0;
    top: 0;
    height: 100%;
    object-fit: cover;
    opacity: 1;
    transition: .4s;
    &.lazyload {
      opacity: 0;
      transform: scale(0.8);
    }
    &.lazyloaded {
      opacity: 1;
      transform: scale(1);
      transition: all 700ms;
    }
    &.hid{
      opacity: 0;
      display: none;
    }
  }

  video{
    width: 100%;
    display: block;
  }

  .video-mute{
    position: absolute;
    bottom: 32px;
    left: 32px;
    border: 2px solid $color-bright;
    border-radius: 4px;
  }

}

.main-video-slider-banner {
  .main-slider-banner {
    width: 100%;
    height: auto;
    position: absolute;
    left: 100%;
    display: none;
    transition: 1s;
    opacity: 0;

    &.act {
      display: block;
    }

    &.active {
      position: relative;
      left: 0;
      opacity: 1;
    }

    div.slide {
      height: auto;
    }

    .tablet-show {
      display: none;
    }

    .mobile-show {
      display: none;
    }

    /*.slick-dots {

      @media screen and(max-width: 784px) {
        bottom: 105px;
      }
      @include media-query($mobile) {
        bottom: 58px;
      }
      @media screen and(max-width: 380px) {
        bottom: 88px;
      }
    }*/

    @media screen and (max-width: 950px) {
      background-color: #ffffff;
      .slide {
        background-image: none !important;

        .slideshow-content-wrapper.page-width {
          flex-direction: column-reverse;
          padding: 0;
          margin: 0 0 32px;

          .slideshow__text-content {
            width: 100%;
            max-width: 100%;
            grid-template: 1fr 32px 48px / 1fr;
            padding: 24px 0 24px;

            h2 {
              max-width: 100%;
              display: flex;
              justify-content: center;
              text-align: center;
              font-size: 48px;
            }
          }

          .slideshow__picture-content {
            width: 100%;
            grid-template: 1fr 32px / 1fr 1fr 1fr 1fr 1fr 1fr;

            .stripes {
              right: 0;
              width: 100%;
              left: 100%;
            }

            img {
              width: 100%;

              &.small-image-meal {
                max-width: 100%;
                width: 100%;
              }
            }
            &.slideshow__picture-content-without-stipes {
              width: 100%;
              margin: 0 0 32px;
              display: block;

              img {
                width: 100%;
              }
            }
          }

          .slideshow__button-wrapper {
            margin-bottom: 50px;
            width: 100%;
            text-align: center;
          }
        }

        .tablet-show {
          display: block;
        }

        .tablet-hid {
          display: none;
        }
      }
    }

    @media screen and (max-width: 650px) {
      .slide {
        .slideshow-content-wrapper.page-width {
          .slideshow__picture-content {

          }

          .slideshow__text-content {
            .slideshow__main-title {
              text-align: center;

              h2 {
                font-size: 24px;
                display: inline-block;
                text-align: center;
                margin: 0 auto;
              }
            }
          }

        }
      }
    }

    @media screen and (max-width: 550px) {
      .slideshow-section {
        .slide {
          .slideshow-content-wrapper.page-width {
            margin: 0 0 16px;

            .slideshow__picture-content {
              display: block;

              img.small-image-meal {
                object-fit: contain;
                max-height: 250px;
              }

              .stripes .stripe {
                padding: 8px;
                p{
                  font-size: 14px;
                }

                &.stripe-active .stripe-item{
                  background-size: cover;
                  max-width: 200px;
                  background-position-x: right;
                  left: 0;
                }
              }
            }
          }
        }
      }
    }

    @media screen and (max-width: 450px){
      .slideshow-section {
        .slide {
          .slideshow-content-wrapper.page-width {

            .slideshow__picture-content {
              img {
                &.small-image-meal {
                  max-height: 180px;
                }

                &.mobile-hid {
                  display: none;
                }

                &.mobile-show {
                  display: block;
                }
              }
            }
            .slideshow__text-content {
              display: block;
              padding: 16px 0;
              .slideshow__main-title{
                margin: 8px;
              }
