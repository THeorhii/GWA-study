  .slick-prev,
  .slick-next{
    width: auto;
    height: auto;

    &:before {
      content: '';
      display: inline-block;
      width: 20px;
      height: 20px;
      border: 2px solid transparent;
      border-right: 2px solid #010101;
      border-bottom: 2px solid #010101;
      margin-left: 7px;
