@media  (max-width: 965px) {
  .collections-section .collections-wrapper .custom-grid {
    flex-wrap: nowrap;
    margin-top: 32px;
    padding-left: 0;
  }
  .main-information .custom-tab__content {
    width: 100%;
    margin: 40px -60px 30px 0;
  }
  .main-information .custom-tab__subtitle p {
    margin-bottom: 0;
  }
  .main-information .tab__subtitle {
    font-size: 18px;
  }
