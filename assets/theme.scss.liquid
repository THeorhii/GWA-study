  color: var(--color-dark-text);
}
.blogs-container .pagination .prev, .blogs-container .pagination .next {
  font: bold calc(var(--font-size-base) + 6px) var(--font-stack-body);
  padding: 0 15px 5px;
}
.blogs-container .pagination .prev a, .blogs-container .pagination .next a {
  color: var(--color-light-grey);
}
.blogs-container .pagination .current {
  border: solid 1px var(--color-yellow) !important;
  background-color: var(--color-yellow);
}
