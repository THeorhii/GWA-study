/*================ How it works ================*/
.how-it-works{
  padding: 16px 32px;
  background-color: white;
  section{
    display: flex;
    justify-content: space-between;
    max-width: 1280px;
    flex-wrap: wrap;
    margin: 0 auto 16px;
  }

  .step-content{
    max-width: 624px;
    width: 50%;
    display: flex;
    margin-bottom: 56px;
    &:nth-child(odd){
      margin-right: 16px;
    }
    &:nth-child(even){
      margin-left: 16px;
    }
  }
  .step-content__image {
    width: 200px;
    height: 200px;
    border-radius: 50%;
    /*border: 2px solid $color-bright;*/
    overflow: hidden;
    position: relative;
    z-index: 1;
    img{
      width: 99%;
      height: 99%;
      object-fit: cover;
    }
  }
  .step-content__image-border {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: auto;
  }
  .step-content__information{
    width: calc(100% - 206px);
    padding-left: 8px;
    position: relative;

    h4{
      font-family: $font-stack-header;
      font-size: 24px;
      font-weight: bold;
      line-height: 1.2;
      color: #010101;
      margin-bottom: 4px;
      margin-top: 0;
    }
    p{
      font-family: $font-stack-body;
      font-size: 14px;
      line-height: 1.5;
      color: #181818;
      margin-top: 0;
    }
    .step-number-wrapper{
      display: flex;
      background-color: #010101;
      width: 55%;
      position: absolute;
      left: -32px;
      bottom: 32px;
      z-index: 0;
      align-items: center;
      justify-content: center;
      clip-path: polygon(0px 100%, 0 0, 100% 0%, 85% 100%);
      .step-number{
        font-family: $font-stack-header;
        font-size: 36px;
        font-weight: bold;
        line-height: 1.56;
        letter-spacing: 1px;
        text-align: center;
        color: #ffffff;
        margin: 8px;
      }
