  @include media-query($tablet) {
    .all-photo{
      flex-wrap: wrap;
      .photo{
        width: 44%;
        margin: 0 8px 16px;
      }
    }
  }
  @include media-query($mobile) {
    padding: 0 17px;
    h3{
      margin-bottom: 16px;
    }
    .all-photo{
      margin-top: 0;
      margin-bottom: 9px;
      .photo{
        svg{
          width: 16px;
          height: 16px;
        }
      }
    }
  }
}
