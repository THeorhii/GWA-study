  @media screen and (max-width: 1295px) {
    width: 96%;
    left: 2%;
    top: 56px;
    bottom: 24px;
    transform: translate(0%, 0%);
    height: auto;
  }

  @media screen and (max-width: 800px) {
    .product-pop-up-img{
      margin-left: 16px;
      margin-right: 16px;
    }
    .product-pop-up-content{
      padding-right: 16px;
    }
  }

  @media screen and (max-width: 650px) {
    top: 2%;
    max-height: 96%;
    .collection-product-pop-up-content{
      overflow-y: auto;
      height: calc(100% - 168px);
      width: 100%;
      display: block;
    }
    .product-pop-up-img {
      margin: 0;
      border: 16px white solid;
      padding: 0;
      max-width: 100%;
      border-top: 0;
    }
    .product-pop-up-content{
      padding-left: 16px;
    }
    .main-product-content {
      height: calc(100% - 124px);
      overflow-y: visible;
      border: none;
    }
    .product-variants{
      position: fixed;
      width: 92%;
      bottom: 72px;
    }
    .product-pop-up-add{
      position: fixed;
      bottom: 0;
    }

    .nutrition-content{
      flex-direction: column;
      .nutrition-circle{
        margin: 0 auto;
      }
      .nutrition-lines{
        margin: 0 auto;
        padding-left: 0;
        .stripe-protein,
        .stripe-carbs,
        .stripe-fat{
          max-width: 100%;
        }
