@media (max-width:650px) {
  .main-information .custom-tabcontent-container {
    padding: 0;
  }
  .custom-tabcontent-collection .tab {
  flex-flow: column-reverse;
  }
  .custom-tabcontent-collection .custom-tab__content {
    margin: -6% auto 0;
    width: 90%;
  }

