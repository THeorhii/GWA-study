    border-left: 1px solid #fff;
    &:nth-last-of-type(1) {
      border-right: 1px solid #fff;
    }
    &.has-active-filter {
      .filter-icon-minus, .filter-icon-plus {
        path{
          fill: $color-yellow;
        }
      }
      .collection-filter-btn {
        color: $color-yellow;
      }
