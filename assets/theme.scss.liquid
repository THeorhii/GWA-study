        }
      }
    }
  }
}

.notes-header{
  font-family: $font-stack-body;
  font-weight: bold;
  text-transform: uppercase;
  font-size: 16px;
  letter-spacing: 1px;
  color: #010101;
  margin: 8px 0;
}

.all-question__answer-buttons{
  display: flex;
  flex-wrap: wrap;

  label{
    width: calc( ( 100% - 32px ) / 3 );
    cursor: pointer;
    &:not(:last-child){
      margin-right: 16px;
    }
    &:nth-child(3){
      margin-right:0;
    }
  }
  p{
    border-radius: 4px;
    border: solid 2px #fbb03a;
    font-family: $font-stack-header;
    font-size: 18px;
    font-weight: bold;
    letter-spacing: 1px;
    text-align: center;
    color: #010101;
    text-transform: uppercase;
    padding: 7px 46px 6px 45px;
    margin-top: 0;
    min-height: 60px;
    display: flex;
    align-items: center;
    justify-content: center;
  }
  input{
    display: none;
    &:checked + p{
      background: $color-bright;
    }
  }
  @media screen and (max-width: 850px){
    p{
      padding: 8px;
    }
  }
  @media screen and (max-width: 480px){
    flex-direction: column;
    label{
      width: 100%;
      &:not(:last-child){
        margin-right: 0;
      }
    }
  }
}

.all-question__answer-checkboxes{
  label >div{
    position: relative;
    font-family: $font-stack-header;
    font-size: 18px;
    letter-spacing: 1px;
    color: #424242;
    padding-left: 30px;
    cursor: pointer;
  }
  label >div:before {
    content: '';
    display: block;
    position: absolute;
    width: 16px;
    height: 16px;
    transform: translateY(-50%);
    top: 50%;
    left: 8px;
    border: 1px solid #dcdcdc;
    transition: .2s;
  }
  label >div:after{
    content: ' \2714';
    display: block;
    position: absolute;
    width: 12px;
    height: 12px;
    transform: translateY(-50%);
    top: 40%;
    left: 10px;
    color: $color-bright;
    transition: .2s;
    opacity: 0;
  }

  input{
    display: none;
    &:checked + div{
      color: $color-bright;
      &:before {
      border-color: $color-bright;
    }
      &:after {
        opacity: 1;
      }
    }
  }

  &.answers-row{
    display: flex;
  }

  @media screen and (max-width: 850px){
    &.answers-row{
      flex-wrap: wrap;
    }
  }
}

