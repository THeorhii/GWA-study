/*===============================================
*   Pop-up WM-360-form
* ================================================*/

.pop-up-wm-360-form{
  position: fixed;
  z-index: 1000;
  top: 24px;
  bottom: 36px;
  left: 50%;
  margin-right: -50%;
  transform: translate(-50%, 0%);
  max-width: 832px;
  width: 100%;
  box-shadow: 0 2px 16px 0 rgba(33, 43, 54, 0.08), 0 31px 41px 0 rgba(33, 43, 54, 0.2);
  background-color: #FFFFFF;
  border-radius: 4px;
  overflow: hidden;

  label{
    position: relative;
    display: block;
    width: 100%;
    margin-bottom: 20px;
  }

  input[type="number"]::-webkit-outer-spin-button,
  input[type="number"]::-webkit-inner-spin-button {
    -webkit-appearance: none;
    margin: 0;
  }
  input[type="number"] {
    -moz-appearance: textfield;
  }
  input[type="number"]:hover,
  input[type="number"]:focus {
    -moz-appearance: number-input;
  }
  input[type=number]::-webkit-inner-spin-button,
  input[type=number]::-webkit-outer-spin-button {
    -webkit-appearance: none;
    margin: 0;
  }

  .checked-required{
    input:invalid {
      border: 2px solid $color-coca-cola;
    }
    input:valid {
      border: 2px solid black;
    }
  }

  header{
    padding: 8px 32px;
    position: relative;
    background: $color-bright;
    h3{
      font-size: 32px;
      padding: 0 32px;
      margin: 0;
    }
    button{
      position: absolute;
      top: 16px;
      right: 32px;
      border: none;
      background-color: transparent;
    }
    svg.icon.icon-close {
      width: 20px;
      height: 20px;
      color: #ffffff;
    }
  }
  main,
  .first-section-questions{
    padding: 16px 32px;
    overflow-y: auto;
    height: calc( 100% - 176px );
  }
  footer{
    padding: 32px 0;
    margin: 0 32px;
    border-top: 1px solid $color-bright;
    display: flex;
    justify-content: flex-end;
    .btn{
      font-family: $font-stack-header;
      font-size: 18px;
      font-weight: bold;
      letter-spacing: 1px;
      text-align: center;
      text-transform: uppercase;
      color: #010101;
      max-width: 168px;
      width: 100%;
      padding: 17px 0 16px 0;
      border-radius: 4px;
      border: solid 2px #fbb03b;
    }
    .btn-close-form{
      background: #ffffff;
      margin-right: 32px;
    }
    .btn-save-form{
      background: $color-bright;
    }
  }

  @media screen and (max-width: 850px){
    width: 96%;

    header{
      padding: 8px 24px;
      h3{
        padding: 0 8px;
      }
      button{
        top: 8px;
        right: 0;
      }
    }

    main,
    .first-section-questions{
      padding: 16px 12px;
    }
  }

  @media screen and (max-width: 480px) {
    header h3 {
      font-size: 24px;
    }

    main,
    .first-section-questions{
      height: calc(100% - 132px );
    }

    label{
       margin-bottom: 8px;
     }

    footer {
      padding: 16px 0;
      margin: 0 8px;
      justify-content: center;

      .btn {
        padding: 12px 16px;
      }

      .btn-close-form {
        margin-right: 8px;
      }
    }
  }
}

.pop-up-wm-360-form.static-form{
  position: static;
  transform: none;
  margin: 40px auto;
  &.checked-required{
    input:invalid {
      border: 2px solid $color-coca-cola;
    }
    input:valid {
      border: 2px solid black;
    }
