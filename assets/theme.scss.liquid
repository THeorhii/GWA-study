/*===============================================
*   Product card -> collection-product-block
* ================================================*/
.collection-product-block {
  position: relative;
  max-width: 368px;
  text-align: left;
  font-family: $font-stack-body;
  font-weight: normal;
  border-bottom-left-radius: 32px;
  overflow: hidden;
  margin: 0 auto;
  .content {
    .content-product-add {
      background-color: #010101;

      .product-price {
        color: #f9f9f9;
      }
    }
  }
  @media screen and (min-width: 650px) {
    &:hover {
      .product-label {
        background-color: rgba(255, 220, 160, 0.9);
        backdrop-filter: blur(30px);
        -webkit-backdrop-filter: blur(30px);
      }

      .content {
        .content-product-add {
          background-color: $color-bright;

          .product-content-add {
            background-color: #010101;
            color: $color-bright;
          }

          .product-content-count {
            background-color: #010101;
            color: $color-bright;
            font-weight: bold;

            button {
              background-color: #010101;
            }

            path {
              fill: $color-bright;
            }
          }
        }

        .product-price {
          color: #010101;
          @media screen and (max-width: 480px) {
            color: #ffffff;
          }
        }
      }

      .product-block-image {
        &::before {
          content: '';
          position: absolute;
          top: 0;
          left: 0;
          right: 0;
          bottom: 0;
          background: #fbb03b;
          opacity: 0.8;
          border-radius: 0 32px 0 0;
          @media screen and (max-width: 480px) {
            border-radius: 0 0 0 0;
          }
        }

        &::after {
          content: 'View Meal';
          position: absolute;
          top: 50%;
          left: 50%;
          transform: translate(-50%, -50%);
          font-family: futura-pt-condensed, Arial, sans-serif;
          font-size: 14px;
          font-weight: bold;
          letter-spacing: 1px;
          text-align: center;
          text-transform: uppercase;
          color: #181818;
          border: 1px solid;
          border-radius: 4px;
          padding: 4px 16px;
        }

        .product-block-nutrition {
          opacity: 1;
        }
      }

    }
  }

  .product-block-image {
    height: 295px;
    border-top-right-radius: 32px;
    overflow: hidden;
    position: relative;
    cursor: pointer;
    border: solid 1px #424242;

    img {
      width: 100%;
      display: block;
      height: 100%;
      object-fit: cover;
    }

    &:hover .product-block-nutrition {
      opacity: 1;
    }

    &:hover::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background: $color-bright;
      opacity: 0.8;
      border-radius: 0 32px 0 0;
      @media screen and (max-width: 650px) {
        border-radius: 0 0 0 0;
      }
    }

    &:hover::after {
      content: 'View Meal';
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      font-family: $font-stack-header;
      font-size: 18px;
      font-weight: bold;
      letter-spacing: 1px;
      text-align: center;
      text-transform: uppercase;
      color: #181818;
    }
  }

  .content {
    background: rgba(245, 245, 245, 0.6);
    backdrop-filter: blur(30px);
    -webkit-backdrop-filter: blur(30px);
    border: solid 1px #424242;
    border-top-color: transparent;
    transition: .4s;
    overflow: hidden;
    padding: 10px 0 0;
    font-family: $font-stack-body;
    border-bottom-left-radius: 32px;


    > div {
      padding: 1px 24px 2px;
      display: flex;
      justify-content: space-between;
      align-items: center;
    }

    .product-content-add {
      width: 120px;
      height: 32px;
      background-color: $color-bright;
      text-transform: uppercase;
      font-family: $font-stack-header;
      font-weight: bold;
      letter-spacing: 1px;
      font-size: 14px;
      border-radius: 4px;
      border: none;
      padding-top: 2px;
      outline: none;
    }

    .product-content-count {
      display: flex;
      align-items: center;
      width: 120px;
      background-color: #fbb03b;
      justify-content: space-between;
      border-radius: 4px;
      padding: 1px 14px;
      text-align: center;
      color: $color-black;
      font-family: $font-stack-header;
      font-size: 14px;
      letter-spacing: 1px;

      button {
        border: none;
        background-color: #fbb03b;
        border-radius: 4px;
        padding: 5px 10px;
        outline: none;

        svg {
          width: 16px;
        }
      }
    }

    .product-price {
      font-size: $font-stack-body;
      color: #010101;
      font-weight: bold;
      transition: .4s;
    }
  }

  .product-title {
    font-family: $font-stack-body;
    font-size: 18px;
    line-height: 22px;
    margin: 0 24px 16px;
    height: 44px;
    display: flex;
    align-items: center;
    color: #010101;

    a {
      text-decoration: none;
      color: #010101;
    }
  }

  .allergens {
    margin: 0 24px;
    color: #181818;
    height: 24px;

    .allergen {
      position: relative;
      margin-right: 8px;

      .icon {
        width: 24px;
        height: 24px;
      }

      span {
        position: absolute;
        top: -17px;
        left: 0;
        opacity: 0;
        width: 150px;
        transition: opacity 0.5s;
        font-size: 14px;
      }

      &:hover {
        path {
          fill: $color-bright;
        }

        span {
          opacity: 1;
        }
      }

      &.vegan-icon {
        svg {
          position: absolute;
        }
      }
    }
  }

  .product-variants {
    padding: 8px 12px 0;
    display: flex;
    justify-content: space-between;
    text-transform: uppercase;
    margin-top: 10px;
    border-bottom: 1px solid #424242;
    margin-bottom: 0;

    > span {
      margin-right: 16px;
    }

    span {
      cursor: pointer;
      letter-spacing: 1px;
      font-size: 12px;
      color: #181818;
      padding-bottom: 8px;
      border: 2px solid transparent;
      position: relative;
      font-weight: bold;

      &.active {
        color: $color-bright;
        border-bottom-color: $color-bright;
      }

      &.active span {
        color: $color-bright;
      }

      &.product-variant-weight {
        margin-left: 4px;
        text-transform: capitalize;
      }

      &.variant-count {
        position: absolute;
        top: -5px;
        font-size: 12px;

        span {
          padding: 0 4px;
          border-radius: 50%;
          background-color: $color-bright;
          color: #181818;
          line-height: 0;
          border: 0;
          font-weight: bold;
        }
      }

    }
  }

  .product-label {
    width: 94px;
    height: 32px;
    border-radius: 4px;
    border: solid 1px #fbb03a;
    background: rgba(251, 176, 59, 0.50);
    backdrop-filter: blur(8.0px);
    -webkit-backdrop-filter: blur(8.0px);
    color: #010101;
    position: absolute;
    top: 16px;
    left: 16px;
    text-align: center;
    padding-top: 8px;
    font-family: $font-stack-body;
    margin-top: 0;
    font-size: 12px;
    letter-spacing: 0;

    &:before {
      content: "";
      width: 8px;
      height: 8px;
      background: #fbb03a;
      border-radius: 50%;
      position: absolute;
      left: 4px;
      top: 12px;
    }
    &.product-label__horizonal{
      left: auto;
      right: 16px;
      width: 111px;
      border-color: #00bc10;
      padding-left: 6px;
      &:before {
        background: #00bc10;
      }
    }
  }

  .product-block-nutrition {
    position: absolute;
    top: 64px;
    right: 16px;
    width: 67px;
    background-color: rgba(255, 220, 160, 0.9);
    backdrop-filter: blur(30px);
    -webkit-backdrop-filter: blur(30px);
    border-radius: 4px;
    font-family: $font-stack-body;
    font-size: 12px;
    line-height: 1.33;
    text-align: center;
    color: #ffffff;
    opacity: 0;
    transition: 0.2s;
    letter-spacing: 0;
    padding: 8px 0;
    @media screen and(max-width: 800px) {
      opacity: 1;
    }

    &:hover {
      opacity: 1;
    }

    p {
      margin: 0;
      color: #181818;
    }

    hr {
      width: 30px;
      height: 1px;
      border: none;
      background-color: rgba(0, 0, 0, 0.05);
    }
  }

  @media screen and (max-width: 1150px) and (min-width: 966px) {
    .product-label {
      left: 8px;
      &.product-label__horizonal{
        right: 8px;
      }
    }
  }

  @media screen and (max-width: 480px) {
    display: flex;
    overflow: hidden;
    border-radius: 0 16px 0 16px;
    max-width: 100%;
    > div {
      border-radius: unset;
      height: auto;
      max-height: 240px;
      width: 100%;

      &.product-block-image {
        width: 53%;
        border-top-right-radius: 0;
        border-bottom-left-radius: 16px;
      }

      &.content {
        width: 47%;
        border-radius: 0 16px 0 0;
        border-top-color: #42424263;
        background: #ffffff;

        .product-variants {
          border-bottom-color: transparent;
        }

        .content-product-add {
          background-color: transparent;
          .product-price{
            color: #010101;
          }
        }
      }
    }

    &.ad-block svg{
      display: none;
    }

    .product-label {
      background-color: rgba(255, 220, 160, 0.9);
      backdrop-filter: blur(30px);
      -webkit-backdrop-filter: blur(30px);
      top: 4px;
      left: 4px;
      font-size: 10px;
      margin: 0;
      width: auto;
      height: 20px;
      padding: 3px 2px 2px 10px;
      &:before{
        width: 4px;
        height: 4px;
        left: 2px;
        top: 7px;
      }
      &.product-label__horizonal{
        right: 4px;
        width: 80px;
      }
    }
    .product-block-nutrition {
      top: auto;
      border-radius: 0 0 0 16px;
      padding: 2px;
      bottom: 0;
      right: 0;
      left: 0;
      display: flex;
      width: 100%;
      height: 42px;
      font-size: 9px;
      justify-content: space-around;
      align-items: center;

      hr {
        display: none;
      }
    }

    .content {
      padding: 0 0 8px;
      border-top-color: #42424263;

      & > div {
        margin: 0 8px;
        padding: 0;
        flex-direction: column;
        background-color: transparent !important;
      }

      .product-price {
        margin: 0 0 4px;
        font-size: 12px;
        padding-top: 8px;
        text-align: center;
      }

      .product-content-add,
      .product-content-count {
        width: 100%;
        height: 32px;
      }
    }

    .product-title {
      height: 64px;
      margin: 8px;
      letter-spacing: 0;
      font-size: 16px;
      font-weight: bold;

      a {
        text-decoration: none;
        color: #010101;
      }
    }
    .allergens {
      margin: 16px 8px 8px;

      .allergen .icon {
        width: 20px;
        height: 20px;
      }
    }
    .product-variants {
      margin: 8px 20px 0 8px;
      border-bottom: none !important;
      padding: 0;

      > span {
        padding-bottom: 0;
        margin-bottom: 8px;
        margin-right: 0;
        border: none;
        display: flex;
        flex-direction: column;

        &.active {
          border: none;
        }

        .product-variant-weight {
          margin-left: 0;
        }

        .variant-count {
          right: -20px;
        }
      }
    }
  }

  @media screen and (max-width: 370px) {
    .product-title {
      margin: 8px 4px;
      font-size: 14px;
    }
    .product-variants{
      margin-left: 2px;
      span{
        letter-spacing: 0;
      }
    }
  }
}
/*===============================================
