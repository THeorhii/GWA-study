* ================================================*/
.main-video-slider-banner{
  background-color: #010101;
  display: flex;
  height: auto;
  .page-width{
    width: auto;
  }
  .slideshow-section{
    .slide {
      .slideshow-content-wrapper {
        display: flex;
        margin: 64px 32px;
        padding: 0;
        height: 100%;
      }
      .slideshow__text-content{
        display: grid;
        grid-template: 1fr 64px 56px / 1fr 64px;
        width: 50%;

        .slideshow__button-wrapper{
          grid-column: 1/2;
          grid-row: 3/4;
        }
      }
      .slideshow__picture-content{
        display: grid;
        grid-template: 1fr 64px 56px / 1fr 1fr 1fr 1fr 1fr 1fr;
        width: 50%;
        .slideshow__picture-block{
          grid-column: 1/5;
          grid-row: 1/3;
          img{
            width: 100%;
            height: 100%;
            object-fit: contain;
            object-position: top;
          }
        }
        .stripes{
          grid-column: 5/7;
          grid-row: 1/3;
          .stripe{
            width: 100%;
          }
        }
      }
    }
  }
  .slider-link{
    position: absolute;
    top: 0;
    bottom: 0;
    right: 0;
    left: 0;
    height: 100%;
    width: 100%;
    z-index: 1000;
  }
}

@media screen and(max-width: 480px){
  .template-page-about-us{
    .main-video-slider-banner{
      padding-top: 16px;
    }
  }
}

.slideshow-section{
  .slide{
    position: relative;
    background-position: center;
    background-repeat: no-repeat;
    .slideshow__text-content{
      font-family: $font-stack-header;
      font-weight: bold;

      h2{
        font-size: $font-size-base + 56px;
        line-height: 1.25;
        letter-spacing: 3px;
        margin: 0;
        color: $color-yellow;

        &:nth-child(2n){
          color: $color-yellow !important;
        }
      }

      .title-fade-in{
        opacity: 0;
      }

      .slideshow__button-wrapper{
      .btn{
          min-width: 225px;
          &:nth-child(2n){
            margin-left: 30px;
          }
        }
      }

    }
    .slideshow__picture-content{
      position: relative;

      img{
        position: relative;
        z-index: 2;
      }
      .stripes{
        .stripe{
          position: relative;
          opacity: 0;
          width: 350px;
          min-height: 50px;
          padding: 8% 0;
          text-align: center;
          color: #424242;

          p{
            font: $font-size-base - 4px $font-stack-header;
            font-weight: bold;
            letter-spacing: 1px;
            text-transform: uppercase;
            margin-bottom: 0;
            span{
              display: block;
              max-width: 205px;
              margin: 0 auto;
            }
          }
          .stripe-item{
            position: absolute;
            top: 73%;
            right: 50%;
            height: 16px;
            width: inherit;
            background: url("line_point_default_new.png") no-repeat;
            background-size: contain;
          }

        }
        .stripe-active{
          .stripe-item{
            background: url("line_point_focus_new.png") no-repeat;
            background-size: contain;
          }
        }
      }
    }
    .slideshow__picture-content-without-stipes{
      display: flex;
      align-items: center;
      width: 50%;
      img{
        max-width: 100%;
        object-fit: cover;
      }
    }

    video{
      display: block;
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      width: 100%;
      height: inherit;
      margin: 0 auto;
      object-fit: cover;
      object-position: center;
    }
    .video-overlay{
      &::before{
        content: '';
        background-color: rgba(255, 255, 255, 0.51);
        position: absolute;
      }
    }
    .video-content{
      position: absolute;
      bottom: 50px;
      left: 0;
      z-index: 100;
      padding: 0 32px;
      text-align: left;
    }

    .video-play{
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      background: none;
      border: none;
      opacity: 1;
      transition: opacity .3s ease;
      &:focus,
      &:active{
        outline: none;
        border: none;
      }
      &:hover{
        opacity: 1 !important;
      }
    }
    .video-play-active{
      opacity: 0 !important;
    }
    .video-mute{
      position: absolute;
      bottom: 30px;
      right: 30px;
      background: none;
      border: none;
      cursor: pointer;
      svg{
        width: 20px;
        height: 20px;
        fill: #454545;
      }
      &:focus,
      &:active{
        outline: none;
        border: none;
      }
   }
  }
  .stnd-slide{
    .slideshow__text-content{
      h2{
        &:nth-child(2n){
          color: $color-dark-text !important;
        }
      }
      .slideshow__button-wrapper{
        .btn-yellow{
          background: transparent;
          border-color: $color-border-black;
        }
      }
    }
  }

  .slick-dots {
    width: max-content;
    left: 50%;
    bottom: 60px;
    transform: translateX(-50%);
    li{
      display: inline-grid;
      button {
        width: auto;
        height: auto;
        &::before {
          position: static;
          display: block;
        }
      }
    }

    .slick-active {
      button {
        &::before {
          color: transparent;
          border: 1px solid black;
          border-radius: 50%;
          padding: 4px;
          line-height: unset;
          font-size: $font-size-base;
        }
      }
    }
  }

  @include media-query($large-down) {
    .slide{
      min-height: 625px;

      .slideshow__text-content {
        h2 {
          font-size: $font-size-base + 44px;
        }
      }
      .slideshow__picture-content {
        .stripes {
          .stripe {
            padding: 5% 0;
            width: 330px;
            .stripe-item{
              top: 80%;
            }
          }
        }
      }
      .slideshow__picture-content-without-stipes{
        img{
          max-width: 100%;
        }
      }
    }
  }
  @include media-query($wide) {
    .slide{
      .slideshow__text-content{
        h2{
          font-size: $font-size-base + 39px;
        }
      }
    }
  }
  @include media-query($medium-down) {
    .slide{
      min-height: 550px;
    }
  }
  @include media-query($small) {
    .slide{
      min-height: 475px;
    }
    .slick-dots{
      bottom: 10px;
    }
  }
  @include media-query($tablet){
    .slide{
      min-height: 575px;
    }
  }
  @include media-query($mobile){
    .slide{
      min-height: 488px;
    }
  }
}

/*===============================================
*   Section steps with image
* ================================================*/
.steps-with-image-section{
  padding-left: 10px;
  padding-right: 10px;
  background: url("how-it-work-background.png");
  background-size: cover;
  font-family: $font-stack-header;

  h3{
    margin: 56px auto;
  }
  .steps-with-image{
    display: flex;
    flex-direction: row;
    margin-bottom: 104px;
  }
  .steps-slider {
    margin: 32px 13% 0 0;
    max-width: 565px;
    width: 100%;
    height: max-content;
    h4{
      margin-top: 25px;
      margin-bottom: 20px;
    }
    .step-content-wrappper{
      max-width: 320px;
      .step-title-wrappper {
        margin-bottom: 38px;
        p {
          margin: 0 auto;
          font: $font-size-base + 2px $font-stack-body;
          line-height: 1.56;
          color: #424242;
        }
      }
      .btn {
        font-family: $font-stack-help;
        font-weight: 500;
        padding: 17px 83px;
      }
    }
    .slick-list{
      margin-top: 80px;
    }
    .slick-dots{
      top: 0;
      bottom: auto;
      height: fit-content;
      border-bottom: 1px solid #424242;
      li{
        height: 40px;
        width: 40px;
        margin: 0 35px;
        &:first-child{
          margin-left: 0;
        }
        &:last-child{
          margin-right: 0;
        }
        a{
          padding: 4px 12px;
          font-size: $font-size-base + 2px;
          font-weight: bold;
          line-height: 1.78;
          text-align: center;
          color: #424242;
          border: solid 2px #424242;
          border-radius: 50%;
          transition: padding 0.5s ease-in;
        }
      }
      .slick-active{
        width: 84px;
        height: 84px;
        a {
          font-size: $font-size-base + 8px;
          padding: 24px 35px;
          color: #010101;
          border-color: #010101;
        }
      }
    }
  }

  @include media-query($large-down) {
    .grid{
      padding-left: 25px;
      padding-right: 25px;
    }
    .steps-with-image{
      margin-bottom: 56px;
    }
    .steps-slider{
      margin-right: 5%;
    }
  }
  @include media-query($wide) {
    padding-bottom: 25px;
    .steps-slider{
      .step-content-wrappper{
        .step-title-wrappper{
          margin-bottom: 20px;
        }
      }
    }
  }
  @include media-query($medium-down) {
    padding-bottom: 25px;
    .steps-slider{
      max-width: 445px;
      .slick-dots{
        li{
          height: 20px;
          width: 20px;
          margin: 0 7%;
        }
      }
    }
  }
  @include media-query($small) {
    padding: 0;
    background-repeat: no-repeat;
    background-size: 705px 100%;
    background-position: left -22px bottom 0px;

    h3{
      margin-top: 32px;
      margin-bottom: 32px;
    }
    .steps-with-image{
      padding-bottom: 59px;
      margin-bottom: 0;

      .steps-slider {
        width: 47%;
        margin-top: 0;
        margin-right: 3%;

        .step-content-wrappper {

          .btn{
            //margin-bottom: 22px;
            font-family: $font-stack-header;
            font-weight: bold;
          }

          .step-title-wrappper {
            margin-bottom: 24px;

            h4 {
              margin: 24px auto 16px;
              font-size: $font-size-base + 2px;
            }
            p {
              font-size: $font-size-base - 2px;
            }
          }
        }
    }
      .slick-dots{
        display: flex;
        flex-direction: row;
        align-items: center;
        padding-bottom: 16px;

        li{
          display: grid;
          width: 32px;
          height: 32px;
          margin: 0;

          &:not(:last-child){
            margin-right: 10%;
          }

          a{
            padding: 1px 11px;
            font-size: $font-size-base - 2px;
          }
        }
        .slick-active{
          width: 48px;
          height: 48px;

          a{
            transition: none;
            font-size: $font-size-base;
            padding: 8px 20px;
          }
        }
      }
    }
  }
  @include media-query($tablet){
    background-position-x: 0%;

    h3{
      font-size: $font-size-base + 24px;
      padding-bottom: 20px;
    }
    .steps-with-image{
      justify-content: center;
      .steps-slider{
        width: 100%;
        max-width: 400px;

        &+div{
          display: none;
        }
        .step-content-wrappper {
          max-width: inherit;
          text-align: center;
        }
      }
      .slick-dots{
        justify-content: center;
      }
    }
  }
  @include media-query($mobile){
    background-size: 720px 100%;
    background-position-x: 5%;

    h3{
      margin: 32px auto 16px;
      padding: 0;
      font-size: $font-size-base * 2;
    }
    .steps-with-image{
      justify-content: center;
      padding-bottom: 24px;

      .steps-slider{
        width: 100%;
        max-width: 270px;
        margin: 0 auto;

        &+div{
          display: none;
        }
        .step-content-wrappper{
          padding-top: 8px;
          text-align: center;
          .step-title-wrappper{
            margin-bottom: 14px;
            h4{
              margin: 8px auto 2px;
              line-height: 1.78;
              letter-spacing: 1px;
            }
            p{
              font-size: $font-size-base - 4px;
            }
          }
          .small-screen-img{
            display: block;
            margin-bottom: 13px;
            img{
              margin: 0 auto;
            }
          }
        }
        .slick-list{
          margin-top: 65px;
        }
        .slick-dots{
          padding-bottom: 16px;
          li{
            &:not(:last-child){
              margin-right: 24px;
            }
          }
        }
      }
    }
  }
}
