/*========== Meal Pack Goal Collections ==========*/

.banner-meal-pack-goal-collections{
  display: grid;
  grid-template: 1fr 1fr / 1fr 30% 1fr;
  max-height: 686px;

  .banner-collection-info{
    grid-column: 2/4;
    grid-row: 1/3;
    background: url('banner-collection-stripes-right.svg') 0% 0% no-repeat;
    background-size: cover;
    position: relative;
    z-index: 1;
    padding-left: 25%;
    padding-right: 5%;
    display: flex;
    justify-content: center;
    flex-direction: column;
    h2{
      font-family: $font-stack-header;
      font-size: 72px;
      font-weight: bold;
      line-height: 1.25;
      letter-spacing: 3px;
      color: #010101;
      margin-bottom: 32px;
      margin-left: 25%;
    }
    div{
      font-family: $font-stack-body;
      font-size: 18px;
      letter-spacing: 1px;
      line-height: 1.25;
      color: #010101;
      margin-bottom: 24px;
      margin-left: 25%;
    }
  }
  .banner-collection-image{
    grid-column: 1/3;
    grid-row: 1/3;
    width: 100%;
    img{
      width: 100%;
      height: 100%;
      object-fit: cover;
      object-position: top;
      display: block;
    }
  }
  .template-collection-meal-pack-question & {
    .banner-collection-image{
      img {
        object-position: center;
      }
    }
  }
  @media screen and(max-width: 1400px){
    .banner-collection-info{
      h2{
        font-size: 64px;
      }
    }
  }

  @media screen and(max-width: 1100px){
    max-height: 100%;
    grid-template: 1fr 1fr / 1fr 25% 1fr;
    .banner-collection-info{
      h2{
        font-size: 52px;
      }
    }
  }

  @media screen and(max-width: 950px){
    .banner-collection-info{
      h2{
        font-size: 48px;
      }
    }
  }

  @media screen and(max-width: 800px){
    grid-template: 1fr 0.75fr / 1fr 20% 1fr;
    .template-collection-meal-pack-question & {
      .banner-collection-image{
        img {
          max-height: 270px;
        }
      }
    }
    .banner-collection-info{
      padding: 32px;
      grid-column: 1/4;
      grid-row: 2/3;
      display: flex;
      align-items: center;
      justify-content: center;
      flex-direction: column;
      h2{
        margin: 0;
        width: 100%;
        text-align: center;
      }
      div{
        margin-left: 0;
      }
    }
    .banner-collection-image{
      grid-column: 1/4;
      grid-row: 1/2;
    }
  }

  @media screen and(max-width: 650px){
    .banner-collection-info{
      h2{
        font-size: 32px;
      }
    }
  }

  @media screen and(max-width: 450px){
    .banner-collection-info{
      padding: 16px;
      h2{
        font-size: 28px;
      }
      div{
        font-size: $font-size-base;
      }
    }
  }
}

/*========== Meal Pack Goal Collections with smart description ==========*/
.banner-meal-pack,
.snippet-banner-meal-pack{
  position: relative;
}
.banner-meal-pack--image-wrap{
  img{
    width: 100%;
    max-height: 500px;
    object-fit: cover;
  }
}
.banner-meal-pack--image-mobile,
.banner-meal-pack--image-tablet{
  display: none;
}
.banner-meal-pack--content{
  position: absolute;
  top: 45%;
  left: 50%;
  margin-right: -50%;
  transform: translate(-50%,-50%);

  h1{
    font-size: 52px;
    margin: 0;
    p,a{
      margin: 0;
      line-height: 1;
    }
  }
  h3{
    text-align: left;
    font-size: 36px;
    margin: 18px 0 0;
    p,a{
      margin: 0;
      line-height: 1;
    }
  }
}

.banner-meal-pack--content.banner-meal-pack--content-top{
  top: 0;
  transform: translate(-50%,0%);
}
.banner-meal-pack--content.banner-meal-pack--content-bottom{
  transform: translate(-50%,0%);
  top: auto;
  bottom: 0;
  margin-bottom: 10px;
}
.banner-meal-pack--content-link{
  text-decoration: none;
  width: auto;
  padding: 12px 24px 11px 24px;
  border-radius: 4px;
  font-family: $font-stack-header;
  font-size: 18px;
  font-weight: 700;
  letter-spacing: 1px;
  text-align: center;
  text-transform: uppercase;
  transition: .4s;
  display: inline-block;
}
.banner-meal-pack--content-short{
  margin-bottom: 24px;
  font-family: $font-stack-body;
  font-size: 18px;
}
.banner-meal-pack--title{
  text-align: center;
  font-size: 52px;
  font-family: $font-stack-header;
  margin-bottom: 20px;
}
.banner-meal-pack--description-full{
  display: none;
}
.banner-meal-pack--description-btn {
  margin: 0 auto;
  display: table;
  border: none;
  background: none;
  color: grey;
  text-transform: uppercase;
  span:nth-child(2){
    display: none;
  }
  svg{
    width: 20px;
    height: 18px;
    padding-top: 4px;
    fill: grey;
  }
}
.banner-meal-pack--description-btn.show-more{
  svg{
    transform: rotate(180deg);
    padding-top: 0;
    padding-bottom: 4px;
  }
  span:first-child{
    display: none;
  }
  span:nth-child(2){
    display: inline-block;
  }
}
.banner-meal-pack--description{
  text-align: center;
  font-family: $font-stack-body;
  font-size: 18px;
  p{
    margin-bottom: 0;
  }
}
.banner-content__mobile{
  display: none!important;
}
.banner-meal-pack--content-mobile{
  display: none!important;
}
@media screen and (max-width: 950px) and (min-width: 481px){
  .banner-meal-pack--image-desktop{
    display: none;
  }
  .banner-meal-pack--image-tablet{
    display: block;
  }
}
@media screen and (max-width: 480px){
  .banner-meal-pack--image-desktop{
    display: none;
  }
  .banner-meal-pack--image-mobile{
    display: block;
  }
  .banner-meal-pack--content h3 {
    font-size: 26px;
    margin: 10px 0 0;
  }
  .banner-meal-pack--content h1{
    font-size: 34px;
  }
  .banner-content__desktop{
    display: none!important;
  }
  .banner-content__mobile{
    display: block!important;
  }
  .banner-meal-pack--content-mobile{
    display: block!important;
  }
  .banner-meal-pack--content-desktop{
    display: none!important;
  }
}

/*===============================================
*   Social networks Style
* ================================================*/
.social-networks-section{

  h3{
    margin: 57px auto;
  }
  .all-photo{
    display: flex;
    flex-direction: row;
    justify-content: center;
    margin-top: 56px;
    margin-bottom: 56px;

    .photo{
      display: flex;
      position: relative;
      width: 100%;
      margin: 0 16px;

      img{
        display: block;
        width: 100%;
        border-radius: 4px;
        object-fit: cover;
      }

      svg{
        opacity: 0;
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        transition: opacity 0.2s ease-in;
      }

      &::before{
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background-color: transparent;
        border-radius: 4px;
        transition: background-color 0.2s ease-in;
      }

      &:hover{
        svg{
          opacity: 1;
        }
        &::before{
          content: '';
          position: absolute;
          top: 0;
          left: 0;
          right: 0;
          bottom: 0;
          background-color: rgba(251, 176, 59, 0.74);
        }
      }
    }
  }

  @include media-query($small) {
    padding: 0 8px;

    h3{
      margin: 32px auto 24px;
    }
