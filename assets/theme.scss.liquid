      padding-left: 8px;
      padding-right: 8px;
      justify-content: center;

      &:first-child {
        padding-right: 0;
        padding-left: 0;
      }

      .grid__item {
        max-width: 100%;

        &.mobile--one-whole {
          width: 100%;
          padding-right: 0;
          padding-left: 0;
          margin-left: 32px;
          margin-right: 32px;
        }
      }
    }
  }

  @media screen and (max-width: 450px) {
    .grid {
      .grid__item.mobile--one-whole {
        margin-bottom: 16px;
        margin-left: 0;
        margin-right: 0;
      }
    }
  }

}

/* Product pop-up */

.collection-product-pop-up{
