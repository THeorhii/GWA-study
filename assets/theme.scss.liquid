/*===============================================
* contact form Styles
This stylesheet use:
- contact (template)
- ndis (template)
* ================================================*/
.contact-form-section {
  padding-bottom: 104px;
  text-align: center;
}
.contact-form-section h2 {
  margin: 32px auto 56px;
}
.contact-form-section form {
  display: flex;
  flex-direction: column;
  max-width: 1166px;
  width: 100%;
  margin: 0 auto;
  text-align: left;
}
.contact-form-section form > div {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.contact-form-section form input, .contact-form-section form textarea {
  border-radius: 4px;
  border: solid 2px #dcdcdc;
  padding: 24px;
  font: var(--font-size-base) var(--font-stack-body);
  letter-spacing: normal;
  color: var(--color-light-grey);
}
.contact-form-section form input::placeholder, .contact-form-section form textarea::placeholder {
  font: var(--font-size-base) var(--font-stack-body);
  line-height: normal;
  letter-spacing: normal;
  color: #868686;
}
.contact-form-section form input:focus, .contact-form-section form textarea:focus {
  outline: none;
  border-color: var(--color-yellow);
}
.contact-form-section form input:focus::placeholder, .contact-form-section form textarea:focus::placeholder {
  opacity: 0;
}
.contact-form-section form input:focus + label, .contact-form-section form textarea:focus + label {
  opacity: 1;
  transform: translateY(-20px);
}
.contact-form-section form .input-wrapper, .contact-form-section form .textarea-wrapper {
  position: relative;
}
.contact-form-section form .input-wrapper label, .contact-form-section form .textarea-wrapper label {
  position: absolute;
  top: -8px;
  left: 0;
  opacity: 0;
  font: 500 var(--font-size-base) var(--font-stack-body);
  color: var(--color-text-black);
}
.contact-form-section form .input-wrapper {
  width: 48.3%;
  margin-bottom: 40px;
  }
.contact-form-section form .input-wrapper input {
  width: 100%;
  padding-top: 20px;
  padding-bottom: 20px;
}
.contact-form-section form .textarea-wrapper {
  flex-direction: column;
}
.contact-form-section form textarea {
  max-height: 124px;
}
.contact-form-section form .btn {
  max-width: 368px;
  width: 100%;
  margin: 56px auto 0;
  padding: 14px;
  border-color: var(--color-yellow);
  font: bold calc(var(--font-size-base) + 2px) var(--font-stack-header);
  color: #090907;
}
.contact-form-section form .errors {
  margin-bottom: 35px;
}
.contact-form-section form .errors ul {
  list-style: none;
}
.contact-form-section form .errors ul li {
  font: 500 var(--font-size-base) var(--font-stack-body);
  color: var( --color-text-black);
}
@media only screen and (max-width: 750px) {
  .contact-form-section form > div {
    flex-direction: column;
  }
  .contact-form-section form .input-wrapper {
    width: 100%;
  }
  .contact-form-section form .input-wrapper, .contact-form-section form .textarea-wrapper {
    max-width: 550px;
    width: 100%;
    margin-left: auto;
    margin-right: auto;
  }
}
@media only screen and (max-width: 480px) {
  .contact-form-section h2 {
    margin-bottom: 40px;
    font-size: calc(var(--font-size-base) * 2);
  }
  .contact-form-section form input, .contact-form-section form textarea {
    padding: 15px;
  }
  .contact-form-section form .input-wrapper label, .contact-form-section form .textarea-wrapper label {
    top: -1px;
  }
  .contact-form-section form .input-wrapper {
    margin-bottom: 24px;
  }
  .contact-form-section form .input-wrapper input {
    padding-top: 15px;
    padding-bottom: 15px;
  }
  .contact-form-section form textarea {
    max-height: 150px;
  }
  .contact-form-section form .btn {
    margin-top: 32px;
  }
}


/*===============================================
*   Main information
* ================================================*/
.main-information {
  background-color: #f3f3f3;

  h3 {
    margin-top: 56px;
    margin-bottom: 20px;
  }

  .collections-section {
    padding-bottom: 32px;
  }

  .grid {
    margin-left: 4.1%;
  }

  .tabcontent-container {
    width: 100%;
    max-width: 1506px;
    padding: 0 30px;
    margin: 0 auto;
  }

  .tab-collection {
    margin-bottom: 36px;
    background-color: rgba(0, 0, 0, 0.05);

    button {
      display: flex;
      align-items: center;
      justify-content: center;
      position: relative;
      margin: 0 4px;
      min-width: 160px;
      color: #181818;

      &:not(:first-child) {
        &:before {
          content: '';
          display: block;
          height: 100%;
          max-height: 32px;
          position: absolute;
          top: 12px;
          left: 0;
          border-left: 1px solid #dcdcdc;
        }
      }
    }
  }

  .tab__content {
    max-width: 646px;
    width: 100%;
    margin: 40px -11.6% 40px auto;
    padding: 6.7% 16px;
    border-radius: 24px;
    backdrop-filter: blur(30px);
    background-blend-mode: hard-light;
    background-image: linear-gradient(to bottom, rgba(245, 245, 245, 0.9), rgba(245, 245, 245, 0.9));
    box-shadow: 3px 8px 25px #aba7a7;
    z-index: 1;
  }

  .tab__title {
    margin-top: 24px;
    font: bold $font-size-base * 2.25 $font-stack-header;
    line-height: 1.33;
    letter-spacing: 1px;
    text-align: center;
    color: #010101;

    p {
      margin: 0;
    }

    strong {
      color: #fbb03c;
    }
  }

  .tab__subtitle {
    max-width: 534px;
    margin: 24px auto 0;
    font: 500 21px $font-stack-header;
    line-height: 1.43;
    letter-spacing: 1px;
    text-align: center;
    color: #181818;
    text-transform: uppercase;

    p {
      margin: 0;
    }
  }

  img {
    height: 100%;
    object-fit: cover;
    border-radius: 24px;
  }

  @media screen and(max-width: 965px) {
    .tab-collection {
      margin-bottom: 0;
    }
    .tab__content {
      width: 90%;
      margin-left: auto;
      margin-right: auto;
      margin-bottom: -30%;
    }
    .collections-section {
      .grid {
        margin: 0 auto;
      }
    }
  }

  @include media-query($tablet) {
    .collections-wrapper .grid {
      padding: 0;
      margin: 0 auto;
    }
    .tabcontent-container {
      padding: 0 15px;
    }
    .tab-collection {
      button {
        min-width: unset;
        margin: 0 4%;

        &:not(:first-child) {
          &::before {
            display: none;
          }
        }
      }
    }
    .tab__content {
      width: 90%;
      padding-bottom: 12%;
    }
    .tab__title {
      font-size: $font-size-base * 1.875;
    }
    .tab__subtitle {
      margin-top: 16px;
      font-size: $font-size-base + 2;
    }
    img {
      max-height: 500px;
    }
  }

  @include media-query($mobile) {
    .tabcontent-container {
      padding: 0 10px;
    }
    .tab-collection {
      flex-wrap: wrap;
    }
    .tab__content {
      margin-top: 32px;
      padding: 32px 10px;
    }
    .tab__title {
      margin-top: 16px;
      font-size: $font-size-base * 1.5;
    }
    .tab__subtitle {
      font-size: $font-size-base;
    }
    img {
      max-height: 400px;
    }
  }
}
// Main information custom
.main-information .custom-tab__subtitle {
  text-transform: none;

}
.main-information .custom-tab__content {
  padding: 2% 16px;
}
.main-information .custom-tab__subtitle--check p {
