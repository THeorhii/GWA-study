  }
}
/*===============================================
*  Section Product card grid Style
* ================================================*/
.product-card{
  position: relative;
  border: 1px solid transparent;
  border-radius: 0 32px 0 32px;
  font-family: $font-stack-body;
  color: $color-product-text;

  a{
    text-decoration: none;
    color: $color-product-text;
  }

  .product-card__image-wrapper{
    display: flex;
    position: relative;
    img{
      width: 100%;
      max-height: 295px;
      object-fit: cover;
      border-radius: 0 32px 0 0;
    }

    .collection-title-wrapper{
      position: absolute;
      top: 16px;
      left: 16px;
      border-radius: 4px;
      padding: 12px 28px;
      background: $color-background-product;
      z-index: 1;
      span{
        font-size: $font-size-base - 4px;
        text-align: center;
        text-transform: uppercase;
      }
    }
  }
  .prod-info{
    padding-top: 24px;
    border: none;
    border-radius: 0 0 0 32px;

    .prod-title{
      text-align: left;
      font: $font-size-base + 2px $font-stack-body;
      line-height: 1.22;
      letter-spacing: normal;
      color: #ffffff;
    }
    .prod-description{
      display: flex;
      flex-direction: row;
      margin-top: 28px;
      //padding: 0 32px;
      .allergen{
        &:not(:last-child){
          margin-right: 16px;
        }
        &:hover{
          .icon{
            fill: $color-yellow;
          }
        }
        .icon{
          fill: #878787;
        }
      }
    }
  }

  .product-variant{
    margin: 0 auto;
    width: 100%;

    .tab {
      display: flex;
      justify-content: space-between;
      overflow: hidden;
      border-bottom: 1px solid #424242;
      margin-top: 18px;
      padding: 0 32px;

      button {
        display: block;
        font: $font-size-base - 4px $font-stack-body;
        font-weight: 500;
        letter-spacing: 1px;
        text-align: center;
        text-transform: uppercase;
        color: #878787;
        background-color: inherit;
        border: 2px solid transparent;
        outline: none;
        cursor: pointer;
        margin: 0;
        padding: 0 0 8px;
        transition: 0.3s;
      }
      .active {
        color: $color-yellow;
        border-bottom: 2px solid $color-yellow;
      }
    }

    .tabcontent {
      display: none;
      width: fit-content;
      border-top: none;
      text-align: center;
      font: $font-weight-header $font-size-base + 2px $font-stack-body;
      letter-spacing: 1px;
      color: $color-yellow;
    }

    .product-variant-footer{
      display: flex;
      justify-content: space-between;
      align-items: baseline;
      padding-top: 14px;
      padding-bottom: 14px;

      .product-price{
        font-family: $font-stack-header;
      }
      .btn{
        font: $font-size-base - 2px $font-stack-header;
        font-weight: bold;
        letter-spacing: 1px;
        text-align: center;
      }
    }
  }

  .wrap-content{
    padding-left: 32px;
    padding-right: 32px;
  }

  .std-btn{
    padding: 7px 23px;
    font: $font-size-base - 2px $font-stack-header;
    font-weight: bold;
    letter-spacing: 1px;
    text-align: center;
    color: #090907;
  }

  @include media-query($medium-down) {
    .wrap-content{
      padding-left: 16px;
      padding-right: 16px;
    }
    .product-card__image-wrapper{
      .collection-title-wrapper{
        padding: 10px 15px;
      }
    }
    .prod-info{
       .prod-title{
         font-size: $font-size-base;
       }
     }
    .product-variant{
      .tab{
        padding-left: 16px;
        padding-right: 16px;
      }
    }

  }
  @include media-query($medium-down) {
    .prod-info{
      .prod-title{
        font-size: $font-size-base + 2px;
      }
    }
  }
  @include media-query($small) {

    .product-card__image-wrapper{
      .collection-title-wrapper{
        font-size: $font-size-base - 4px;
        padding: 12px 17px;
      }
    }
    .wrap-content{
      padding-right: 24px;
      padding-left: 24px;
    }
    .product-variant{
      .tab{
        padding-left: 24px;
        padding-right: 24px;
      }
      .std-btn{
        min-width: 120px !important;
        padding: 5px 5% !important;
      }
    }

  }
  @include media-query($mobile) {
    border-radius: 0 16px 0 16px;
    max-width: 265px;
    margin: 0 auto;

    .product-card__image-wrapper{

      &:hover{
        &::before{
          border-radius: 0 16px 0 0;
        }
      }
      img{
        max-height: 225px;
        border-radius: 0 16px 0 0;
      }
      .collection-title-wrapper{
        top: 8px;
        left: 8px;
        padding: 8px 11px;
      }
    }
    .prod-info{
      border-radius: 0 0 0 16px;
    }
    .product-variant{
      .product-variant-footer{
        padding-top: 11px;
        padding-bottom: 11px;
        .std-btn{
          padding: 2px 9.5% 3px !important;
        }
      }
      .tabcontent{
        font-size: $font-size-base;
      }
    }
  }
}
/*===============================================
* Section collections Styles
* ================================================*/

.collections-section{
  display: table;
  width: 100%;
  padding-bottom: 104px;
  margin: 0 auto;

  h1{
    margin: 0 auto 40px;
    display: table;
    font-size: 48px;
    @media only screen and (max-width: 750px){
      font-size: 35px;}
    @media only screen and (max-width: 480px){
      font-size: 32px;
    }
  }

  .collections-wrapper,
  .product-recommendations-wrapper{
    .grid{
      display: flex;
      margin-left: -$grid-gutter/2;
      //margin-left: -$grid-gutter;

      .product-card{
        height: 100%;
        display: flex;
        flex-direction: column;

        .prod-info{
          display: flex;
          flex-direction: column;
          flex-grow: 1;

          .prod-title{
            flex-grow: 1;
          }
        }
      }
    }
    .link-to-collection{
      min-width: 316px;
      margin-top: 56px;
      padding-left: 55px;
      padding-right: 55px;
      font-family: $font-stack-header;
    }
  }

  .tab-collection {
    display: flex;
    justify-content: center;
    overflow: hidden;
    background: #f3f3f3;
    border: solid 1px #ececec;
    margin-bottom: 56px;

    button {
      display: block;
      font: $font-weight-header $font-size-base + 2px $font-stack-help;
      letter-spacing: 1px;
      text-align: center;
      text-transform: uppercase;
      color: $color-dark-text;
      //background-color: inherit;
      background-color: transparent;
      border: none;
      outline: none;
      cursor: pointer;

      margin: 0 4%;
      padding: 16px 0;
      transition: 0.3s;
    }
    .active {
      position: relative;
      outline: none;
      border: none;
      color: $color-yellow;

      &::after{
        content: '';
        position: absolute;
        bottom: 0;
        left: 50%;
        transform: translateX(-50%);
        width: 55%;
        border-bottom: 2px solid #fbb03b;
      }
    }
  }

  .tabcontent-collection {
    display: block;
    opacity: 1;
    max-width: $width-site;
    width: 100%;
    margin: 0 auto;
    border-top: none;
    text-align: center;
    font: $font-weight-header $font-size-base + 2px $font-stack-body;
    letter-spacing: 1px;
    color: $color-yellow;
  }


  .show {
    display: block;
    opacity: 1;
    transition: all 2s linear;
  }

  @include media-query($medium-down) {
    .collections-wrapper,
    .product-recommendations-wrapper{
      .grid {
        margin-left: -$grid-gutter/4;

        .grid__item {
          padding-left: $grid-gutter/2;
        }
      }

      .link-to-collection{
        padding-left: 40px;
        padding-right: 40px;
      }
    }
    .link-to-collection{
      padding: 15px 10%;
    }
  }
  @include media-query($small){
    padding-bottom: 46px;

    h3{
      margin-bottom: 16px;
    }

    .collections-wrapper,
    .product-recommendations-wrapper{
      .grid{
        flex-wrap: wrap;
        padding: 0 6%;
        .grid__item{
          margin-bottom: 24px;
          padding-left: 24px;
        }
      }
      .link-to-collection{
        border: none;
        margin-top: 0;
        padding: 9px 6.9% 8px !important;
      }
    }

    .tab-collection{
      margin-bottom: 24px;
      button{
        font-size: $font-size-base;
      }
    }
  }
  @include media-query($tablet){
    .collections-wrapper,
    .product-recommendations-wrapper{
      .grid{
        padding: 0 3%;
        .grid__item{
          margin-bottom: $grid-gutter/2;
          padding-left: $grid-gutter/2;
        }
        .product-card{
          .prod-info{
            padding-top: 16px;
            .wrap-content{
              padding-left: 16px;
              padding-right: 16px;
            }
            .prod-title{
              font-size: $font-size-base;
            }
            .prod-description{
              margin-top: 16px;
            }
          }
          .product-variant{
            .tab{
              padding-left: 16px;
              padding-right: 16px;
            }
            .std-btn{
              min-width: 105px !important;
            }
          }
        }
      }
    }
  }
  @include media-query($mobile){
    h3{
      //max-width: 190px;
      margin-bottom: 8px;
      padding: 0 15%;
    }
    .tab-collection{
      margin-bottom: 16px;
      button{
        padding: 8px 0 7px;
        font-size: $font-size-base - 2px;
      }
    }
    .collections-wrapper,
    .product-recommendations-wrapper{
      .grid{
        padding: 0;
        .product-card{
          .prod-info{
            padding-top: 8px;
            .allergen{
              .icon{
                width: 16px;
                height: 16px;
              }
            }
          }
        }
      }
      .link-to-collection{
        margin-top: 8px;
        padding: 9px 36px 8px !important;
      }
    }
  }
}

