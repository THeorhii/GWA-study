  .custom-spinner-drawer{
    left: calc(50% - 75px);
    top: 250px;
    position: fixed;
    z-index: 2;
    width: 100%;

    > div {
      height: 150px;
      width: 50px;
      display: inline-block;
      background-color: $color-bright;
      border: 1px solid $color-bright;
      -webkit-animation: custom-spinner-loader 1.3s linear infinite;
      animation: custom-spinner-loader 1.3s linear infinite;
      -webkit-transform: scale(0.91);
      transform: scale(0.91);

      &:nth-child(1) {
        -webkit-animation-delay: 0.39s;
        animation-delay: 0.39s;
      }

      &:nth-child(2) {
        -webkit-animation-delay: 0.52s;
        animation-delay: 0.52s;
      }

      &:nth-child(3) {
        -webkit-animation-delay: 0.65s;
        animation-delay: 0.65s;
      }
    }
    @-webkit-keyframes custom-spinner-loader {
      0% {
        -webkit-transform: scale(1.2);
        opacity: 1
      }
      100% {
        -webkit-transform: scale(0.7);
        opacity: 0.1
      }
    }

    @keyframes custom-spinner-loader {
      0% {
        transform: scale(1.2);
        opacity: 1
      }
      100% {
        transform: scale(0.7);
        opacity: 0.1
      }
    }
  }
