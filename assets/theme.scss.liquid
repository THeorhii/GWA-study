.cart-drawer__progress-bar-wrapper{
  padding: 8px 0 12px;
  border-bottom: 1px solid #DCDCDC;

  .cart-drawer__progress-bar{
    padding: 0 24px 22px 16px;
    border-bottom: 1px solid #DCDCDC;
    position: relative;
  }

  .progress-bar__stripe{
    /*width: 100%;*/
    height: 4px;
    border-radius: 2px;
    /*margin: 24px 0;*/
    background: rgb(238, 238, 238);

    width: calc(100% - 40px);
    position: absolute;
    top: 0;
    left: 16px;
  }
  .progress-bar__dots{
    display: flex;
    /*position: absolute;
    top: -8px;*/
    /*width: calc(100% - 32px);*/
    width: 100%;
    left: 0;
    justify-content: flex-end;
    position: relative;
    margin: 24px 0;
  }
  .progress-bar__block-dot {
    width: 25%;
    text-align: center;
    position: relative;
    >div{
      display: flex;
      justify-content: flex-end;

      position: relative;
      right: -8px;
      top: -8px;
    }
    &:last-child{
      padding-right: 0;
      text-align: right;
      .progress-bar__gift-prices,
      .progress-bar__gift-titles{
        right: 0;
      }
    }
    &.unlocked{
      span{
        color: #010101;
      }
      .progress-bar__dot{
        background-image: linear-gradient(to right, #ff6300 0%, #e45900 100%);
        &:after{
          background-color: #e50702;
        }
      }
    }
  }
  .progress-bar__gift-prices,
  .progress-bar__gift-titles{
    font-family: $font-stack-header;
    font-size: 11px;
    font-weight: bold;
    line-height: 1.45;
    color: #b6b6b6;
    text-transform: uppercase;
    display: block;

    position: absolute;
    width: 70px;
    right: -27px;
  }
  .progress-bar__gift-prices {
    top: -15px;
  }
  .progress-bar__gift-titles {
    bottom: -20px;
  }

  .progress-bar__dot{
    position: relative;
    width: 16px;
    height: 16px;
    background-color: #eeeeee;
    display: inline-block;
    top: 2px;
    border-radius: 50%;
    transition: .4s;

    &::after{
      content: '';
      width: 8px;
      height: 8px;
      background-color: #b6b6b6;
      position: absolute;
      top: 4px;
      left: 4px;
      border-radius: 50%;
      transition: .4s;
    }
  }

  h5{
    font-family: $font-stack-header;
    font-size: 18px;
    line-height: 1.78;
    letter-spacing: 1px;
    color: #010101;
    text-transform: uppercase;
    margin: 0 16px;
  }

  .gifts-block {
    display: flex;
    margin: 4px 16px;
  }
  .gifts-block__gift {
    max-width: 72px;
    width: 100%;
    &:not(:last-child){
      margin-right: 16px;
    }
    img{
      width: 100%;
      max-width: 72px;
      max-height: 72px;
      height: 100%;
      object-fit: cover;
      filter: grayscale(0%);
      transition: .4s;
    }
    p{
      font-family: $font-stack-body;
      font-size: 12px;
      text-align: center;
      line-height: 1.5;
      color: #010101;
      margin: 0;
    }
    rect{
      stroke: #00BC10;
    }
    path{
      fill: #00BC10;
    }
    &.locked{
      img {
        filter: grayscale(100%);
        transition: .4s;
      }
      p{
        color: #868686;
      }
      rect{
        stroke: rgba(134, 134, 134, 0.8);
      }
      path{
        fill: #868686;
      }
    }
  }
}

/*===============================================
*   Questionnaire Meal Pack
* ================================================*/
.packs-or-program{
  text-align: center;
  border-bottom: 2px solid #dcdcdc;
  padding-bottom: 40px;
}

.meals-program-on-collection{
  padding-bottom: 40px;
  .wm-360-buy-meals-pack > div{
    border-bottom: 2px solid #dcdcdc;
    padding-bottom: 40px;
  }
}

.btn-meals-pack-quiz{
  max-width: 368px;
  width: 100%;
  height: 56px;
  margin: 16px;
  padding: 17px 4px 16px 4px;
  border-radius: 4px;
  border: solid 2px #010101;
  font-family: $font-stack-header;
  font-size: 18px;
  font-weight: bold;
  letter-spacing: 1px;
  text-align: center;
  color: #010101;
  text-transform: uppercase;
  background-color: #FFFFFF;
  transition: 0.4s;
  &:hover{
    border-color: $color-bright;
    background-color: $color-bright;
  }
  @media screen and(max-width: 480px){
    max-width: 280px;
  }
}

@keyframes full-answer-block {
  100% {background: center / contain no-repeat url('{{ "icon-check.png" | asset_url }}') #010101; border-color: #010101;}
}

@keyframes full-answer-line {
  100% {background: linear-gradient(to right, #010101, #010101 100%);}
}

.test-meal-pack{
  padding-top: 18px;
  padding-bottom: 56px;

  .grid{
    h3{
      padding-bottom: 20px;
      margin-bottom: 0
    }
  }

  .meals-pack-goal-main-subheader {
    font-family: $font-stack-body;
    text-align: center;
    font-size: 18px;
    margin: 0 0 32px;
  }

  .test-content {
    max-width: 1168px;
    margin: 0 auto;
  }

  .test-question{
    width: 100%;
    position: relative;
    margin: 0 auto;
    display: table;
  }

  .question-content{
    padding-left: 0;
    display: flex;
    justify-content: center;
    border-bottom: 2px solid #dcdcdc;
  }

  .question-number{
    display: flex;
    justify-content: center;
    padding-bottom: 32px;
    border-bottom: 2px solid #dcdcdc;
    div{
      display: flex;
      justify-content: center;
      align-items: center;
      height: 30px;
      width: 30px;
      border: 2px solid $color-bright;
      border-radius: 50%;
      position: relative;
      &:not(:last-child){
        margin-right: 210px;
        &::after{
          content: '';
          width: 211px;
          position: absolute;
          background: linear-gradient(to left, #010101, $color-bright 0%, $color-bright 100%);
          left: 28px;
          height: 2px;
        }
      }
      &.active{
        animation: full-answer-block  1s forwards;
        &::after{
          animation: full-answer-line  1.5s forwards 0.6s;
        }
      }
      @media screen and(max-width: 1050px){

      }
      @media screen and(max-width: 650px){
        &:not(:last-child){
          margin-right: 70px;
          &::after{
            width: 72px;
          }
        }
      }
    }
  }

  @media screen and(max-width: 1600px){
    .grid{
      padding-right: 32px;
      padding-left: 32px;
    }
  }

  @media screen and(max-width: 800px){
    .grid{
      padding-right: 24px;
      padding-left: 24px;
    }
  }

  @media screen and(max-width: 650px){
    .grid{
      padding-right: 16px;
      padding-left: 16px;
    }
  }

  @media screen and(max-width: 450px){
    .grid{
      padding-right: 8px;
      padding-left: 8px;
    }
    .test-content-activity{
      padding-right: 0;
    }
    .test-content-days .btn-days{
      float: none;
      max-width: 100%;
      width: 100%;
      padding: 4px 8px 8px;
      >span{
        &:last-child{
          align-items: center;
        }
        &:first-child{
          margin: 0;
        }
        &.pack-price{
          display: inline-block;
        }
        &.meals-count{
          margin: 4px 0 0;
          display: inline-block;
        }
      }
      &:not(:last-child) {
        margin-right: 0;
        margin-bottom: 16px;
      }
    }
    .test-question button{
      bottom: -340px;
      font-size: 14px;
      transition: .5s;
      &.to-activity{
        bottom: -460px;
      }
    }
  }
}

.question-text{
  font-family: $font-stack-header;
  font-size: 24px;
  font-weight: bold;
  letter-spacing: 1px;
  margin-top: 32px;
  margin-bottom: 16px;
  text-align: center;
}

.quiz-go-back{
  color: #868686;
  outline: none;
  text-transform: uppercase;
  background-color: transparent;
  transition: 0.4s;
  margin: 40px auto 16px;
  display: table;
  border: none;
  svg{
    margin-bottom: -9px;
  }

  &:hover{
    color: $color-bright;
    border-color: $color-bright;
    path{
      fill: $color-bright;
    }
  }
}

.test-content-gender{
  margin-bottom: 56px;
  margin-top: 16px;
  width: 100%;
  text-align: center;
}

.test-content-activity{
  display: none;
  width: 100%;
  >div{
    display: flex;
    justify-content: space-between;
    margin-bottom: 56px;
    margin-top: 16px;
    @media screen and(max-width: 650px){
      flex-direction: column;
      margin-bottom: 32px;
    }
    >label{
      width: calc( 100% / 3 - 16px );
      display: inline-grid;

      @media screen and(max-width: 650px){
        width: 100%;
        margin-bottom: 16px;
      }
    }
  }
  .radio-activity{
    &.active,
    &:hover{
      background-color: $color-bright;
      border-radius: 4px;
      .radio__text:before {
        border-color: $color-bright;
        background-color: #FFFFFF;
      }
      .radio__text:after {
        background-color: $color-bright;
      }
      .radio__text {
        background-color: $color-bright;
        border-color: $color-bright;
      }
    }
    input {
      position: absolute;
      z-index: -1;
      opacity: 0;
      margin: 10px 0 0 20px;
      &:checked + .radio__text:before {
        border-color: $color-bright;
        background-color: #FFFFFF;
      }
      &:checked + .radio__text:after {
        background-color: $color-bright;
      }
      &:checked + .radio__text {
        background-color: $color-bright;
        border-color: $color-bright;
      }
    }
    .radio__text{
      position: relative;
      padding: 16px 16px 16px 60px;
      cursor: pointer;
      font-family: $font-stack-header;
      font-size: 24px;
      font-weight: 500;
      letter-spacing: 1px;
      text-transform: uppercase;
      border: 2px solid #010101;
      border-radius: 4px;
      min-height: 118px;
      @media screen and(max-width: 650px){
        text-align: center;
        padding-left: 16px;
        padding-top: 18px;
        font-weight: bold;
        font-size: 18px;
      }
      span{
        display: block;
        font-family: $font-stack-body;
        font-size: $font-size-base;
        line-height: 1.5;
        text-transform: initial;
        margin-top: 8px;
        @media screen and(max-width: 650px){
          margin-top: 12px;
          line-height: 1.2;
          font-weight: normal;
        }
      }
      &:before{
        content: '';
        position: absolute;
        top: 16px;
        left: 16px;
        transition: .2s;
        width: 24px;
        height: 24px;
        border: solid 2px $color-bright;
        border-radius: 50%;
        @media screen and(max-width: 650px){
          left: 25%;
        }
      }
      &:after{
        content: '';
        position: absolute;
        top: 22px;
        left: 19px;
        margin-left: 3px;
        transition: .2s;
        width: 12px;
        height: 12px;
        background-color: transparent;
        border: 1px solid transparent;
        border-radius: 50%;
        @media screen and(max-width: 650px){
          left: calc(25% + 3px);
        }
      }
    }
  }
}

.test-content-days{
  display: none;
  margin-bottom: 56px;
