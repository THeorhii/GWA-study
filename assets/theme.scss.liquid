.subscriptions-pop-up,
.subscriptions-pop-up-canceled{
  position: fixed;
  z-index: 1000;
  top: 50%;
  left: 50%;
  margin-right: -50%;
  transform: translate(-50%, -50%);
  border-radius: 4px;
  background-color: #ffffff;
  padding: 16px;
  max-width: 968px;
  width: 100%;
  @media screen and (max-width: 450px){
    transform: translate(-50%, -45%);
  }

  .close-pop-up-icon{
    border: none;
    background: transparent;
    position: absolute;
    right: 16px;
  }

  .subscription-details-block {
    padding: 24px 32px 32px;
    @media screen and (max-width: 650px) {
      padding: 16px;
    }
  }
  .subscription-details {
    display: flex;
    flex-flow: row wrap;
    justify-content: space-between;
    @media screen and (max-width: 650px) {
      flex-direction: column;
    }
  }
  .details-item {
    flex: 1;
    min-width: 170px;
    margin-bottom: 16px;
    &:first-child{
      margin-right: 16px;
      @media screen and (max-width: 650px) {
        margin-right: 0;
      }
    }
    &:last-child{
      margin-left: 16px;
      @media screen and (max-width: 650px) {
        margin-left: 0;
      }
    }
  }

  .details-item-input {
    padding: 22px 24px;
    line-height: 1;
    border: solid 2px #dcdcdc;
    border-radius: 4px;
    width: 100%;
    font-family: $font-stack-body;
    max-height: 62px;
    outline: none;
    @media screen and(max-width: 450px){
      padding: 8px 24px;
    }
  }
  .next-payment-date {
    color: #868686;
  }
  .change-address {
    margin-top: 16px;
  }
  .change-address-link {
    font-size: 14px;
    font-weight: 500;
    line-height: normal;
    letter-spacing: 1px;
    color: $color-bright;
    font-family: $font-stack-header;
    text-decoration: none;
    text-transform: uppercase;
  }
  .subscriptions-block-title{
    font-family: $font-stack-body;
    font-weight: bold;
    font-size: 16px;
    letter-spacing: 1px;
    color: #181818;
    text-transform: uppercase;
    @media screen and (max-width: 450px) {
      margin: 8px;
    }
  }
  .subscription-action-buttons{
    &.saved{
      display: flex;
      justify-content: space-between;
      @media screen and (max-width: 450px) {
        flex-direction: column;
      }
    }
    p{
      font-family: $font-stack-body;
      font-size: 16px;
      color: $color-coca-cola;
      text-transform: uppercase;
      margin-right: 16px;
      margin-top: 0;
      @media screen and (max-width: 450px) {
        text-align: center;
        font-size: 12px;
      }
    }
    margin-top: 32px;
    @media screen and (max-width: 450px) {
      margin-top: 0;
    }
  }
  .close-subscription-pop-up{
    border-color: $color-bright;
    background: transparent;
    margin-right: 16px;
    width: 168px;
    padding: 16px 40px;
    &:focus{
      border-color: $color-bright;
    }
    @media screen and (max-width: 450px){
      margin: 0 auto 16px;
      display: table;
      width: 100%;
    }
  }
  .activate-subs-btn{
    width: 168px;
    border-color: $color-bright;
    padding: 16px 32px;
    &:focus{
      border-color: $color-bright;
    }
    @media screen and (max-width: 450px){
      margin: 0 auto;
      display: table;
      width: 100%;
    }
  }
  .subscriptions-available-delivery-hours{
      list-style: none;
      width: 100%;
      padding: 0;
      display: flex;
      justify-content: space-between;
      flex-wrap: wrap;
      li{
        border-radius: 4px;
        margin-bottom: 16px;
        width: calc(50% - 16px);
        &.active{
          border-color: $color-bright;
        }
        .radio-hours input {
          position: absolute;
          z-index: -1;
          opacity: 0;
          margin: 10px 0 0 20px;
          &:checked + .radio__text:before {
            border-color: $color-bright;
          }
          &:checked + .radio__text:after {
            background-color: $color-bright;
            border-color: $color-body;
          }
          &:checked + .radio__text{
            border-color: $color-bright;
          }
          &:checked + .radio__text span{
            color: $color-bright;
          }
        }
        .radio__text{
          position: relative;
          cursor: pointer;
          display: flex;
          justify-content: space-between;
          align-items: center;
          padding: 0 8px 0 32px;
          border: solid 2px #dcdcdc;
          span{
            font-family: $font-stack-body;
            line-height: 2;
            &:first-child{
              font-size: $font-size-base;
              letter-spacing: 1px;
              color: #010101;
            }
            &:last-child{
              font-size: 14px;
              color: #424242;
            }
          }
          &:before{
            content: '';
            position: absolute;
            top: 8px;
            left: 6px;
            transition: .2s;
            width: 16px;
            height: 16px;
            border: solid 1px #dcdcdc;
            background-color: #ffffff;
            border-radius: 50%;
          }
          &:after{
            content: '';
            position: absolute;
            top: 10px;
            left: 8px;
            transition: .2s;
            width: 12px;
            height: 12px;
            background-color: transparent;
            border: 1px solid transparent;
            border-radius: 50%;
          }
        }
      }

  }
  /* Delivery hours */
  .delivery-hours-block {
    display: flex;
    flex-flow: row wrap;
    justify-content: space-between;
    font-family: $font-stack-body;
    .subscriptions-block-title {
      width: 100%;
    }
    .delivery-hours-block-item {
      position: relative;
      margin-bottom: 16px;
      width: calc(50% - 16px);
      input {
        display: none;
      }
      label {
        padding: 8px 16px 9px 40px;
        border-radius: 4px;
        border: 2px solid #dcdcdc;
        display: flex;
        flex-flow: row wrap;
        justify-content: space-between;
        align-items: center;
        transition: all 0.3s;
        cursor: pointer;
        span {
          display: block;
          line-height: 1.2;
        }
        .delivery-hours-title {
          font-size: 16px;
          color: #868686;
        }
        .delivery-hours-info {
          color: #868686;
          font-size: 12px;
        }
      }
      input:checked ~ label {
        border-color: $color-yellow;
        .delivery-hours-title {
          color: $color-border-black;
        }
        .delivery-hours-info {
          color: $color-border-black;
        }
      }
    }
    @media screen and (max-width: 800px){
      label {
        .delivery-hours-info {
          width: 100%;
        }
      }
    }
    @media screen and (max-width: 650px) {
      .delivery-hours-block-item {
        width: 100%;
      }
    }
  }

  .delivery-radio-btn {
    position: absolute;
    width: 16px;
    height: 16px;
    border-radius: 50%;
    border: 1px solid #c4cdd5;
    transform: translateY(-50%);
    top: 50%;
    left: 12px;
    transition: all 0.2s;
    pointer-events: none;
    &:after {
      content: '';
      display: block;
      position: absolute;
      width: 8px;
      height: 8px;
      border-radius: 50%;
      transform: translate(-50%, -50%);
      top: 50%;
      left: 50%;
      background-color: $color-yellow;
      opacity: 0;
      transition: all 0.2s;
    }
  }
  input:checked ~ .delivery-radio-btn {
    border-color: $color-yellow;
    &:after {
      opacity: 1;
    }
  }
}

