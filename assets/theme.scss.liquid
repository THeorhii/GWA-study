/*===============================================
* Login Styles
* ================================================*/
.template-login,
.template-register,
.template-reset_password,
.template-activate_account{
  #MainContent{
    background: center / cover no-repeat url("bg-customers.png");
    #CustomerLoginForm,
    #CustomerRegisterForm,
    #RecoverPasswordForm,
    #ResetPasswordForm,
    #CustomerActivateAccountForm{
      min-height: 770px;
      max-width: 832px;
      width: 100%;
      margin: 0 auto;
      padding: 0 32px;
      h1,
      h2{
        font: bold $font-size-base * 3 / normal $font-stack-header;
        letter-spacing: 1px;
        text-align: center;
        color: $color-dark-text;
        &+p{
          margin: 0 auto 56px;
          font: normal $font-size-base + 12px $font-stack-body;
          line-height: 1.14;
          letter-spacing: normal;
          text-align: center;
          color: #424242;
        }
      }
      h2{
        margin-bottom: 16px;
        //&+p{
        //  margin: 0 auto 56px;
        //  font: normal $font-size-base + 12px $font-stack-body;
        //  line-height: 1.14;
        //  letter-spacing: normal;
        //  text-align: center;
        //  color: #424242;
        //}
      }
      .invited-message{
        margin: 0 auto 24px;
        font: normal $font-size-base + 12px $font-stack-body;
        line-height: 1.14;
        letter-spacing: normal;
        text-align: center;
        color: $color-coca-cola;
        a{
          text-decoration: underline;
          cursor: pointer;
          color: $color-coca-cola;
        }
      }

      form{
        display: flex;
        flex-direction: column;
        justify-content: center;
        text-align: center;
        label{
          margin-bottom: 8px;
          font: bold $font-size-base / normal $font-stack-body;
          letter-spacing: 1px;
          text-align: left;
          text-transform: uppercase;
        }
        input{
          outline: none;
          border: solid 2px #dcdcdc;
          border-radius: 4px;
          background-color: #ffffff;
          padding: 21px 24px;
          margin-bottom: 32px;
          font: $font-size-base / normal $font-stack-body;
          letter-spacing: normal;
          &:focus,
          &:active{
            border-color: $color-yellow;
          }
          &::placeholder{
            color: #868686;
          }
        }
        input[type="submit"]{
          max-width: 368px;
          width: 100%;
          margin: 0 auto 32px;
          padding-top: 16px;
          padding-bottom: 16px;
          border-color: $color-yellow;
          background-color: $color-yellow;
          font: bold $font-size-base +2px $font-stack-header;
          text-transform: uppercase;
        }
        .input-error{
          border: 1px solid #e02323;
        }
        .CustomerPassword-wrapper{
          position: relative;
          display: flex;
          flex-direction: column;
        }
        .showpass{
          position: absolute;
          top: 50%;
          right: 24px;
          width: 16px;
          height: 12px;
          cursor: pointer;
          transform: translateY(-50%);
          background: center / contain no-repeat url("hide-icon.svg");
        }
        .showpass-active{
          background: center / contain no-repeat url("view-icon.svg");
        }
        .form-links{
          margin-bottom: 32px;
          a{
            padding: 0 8px;
            font: bold $font-size-base - 2px $font-stack-header;
            letter-spacing: 1px;
            text-align: center;
            text-transform: uppercase;
            text-decoration: none;
            color: #868686;
            &:not(:last-child){
              border-right: 1px solid #868686;
            }
          }
        }
        .registerBtn-wrapper{
          display: flex;
          flex-direction: row;
          justify-content: space-between;
          .linkToHomepage,
          .cancelBtn{
            display: block;
            max-width: 368px;
            width: 100%;
            margin: 0 0 32px;
            padding-top: 16px;
            padding-bottom: 16px;
            border: solid 2px #010101;
            border-radius: 4px;
            background-color: transparent;
            font: bold 18px futura-pt-condensed,Arial,sans-serif;
            text-transform: uppercase;
            text-decoration: none;
            color: #010101;
          }
          input[type="submit"]{
            margin-right: 0;
            margin-left: 0;
            &.centre-input-position{
              margin: 0 auto;
            }
          }
        }
        .errors{
          ul{
            list-style: none;
            padding-left: 0;
            li{
              font: bold $font-size-base + 2px $font-stack-header;
              letter-spacing: 1px;
              text-align: center;
              text-transform: uppercase;
              color: #c30202;
            }
          }
        }
      }

      @include media-query($small){
        form{
          .CustomerPassword-wrapper,
          input,
          label{
            width: 90%;
            margin-left: auto;
            margin-right: auto;
          }
          .CustomerPassword-wrapper{
            label{
              width: 100%;
            }
          }
          input[type="password"],
          .inputPassword{
            width: 100%;
          }
          .registerBtn-wrapper{
            flex-direction: column-reverse;
            justify-content: center;
            margin-bottom: 56px;
            .linkToHomepage,
            .cancelBtn,
            input[type="submit"]{
              margin: 0 auto 32px;
            }
          }
        }
      }
      @include media-query($tablet){
        min-height: 650px;
      }
      @include media-query($mobile){
        min-height: 550px;
        padding: 0 24px;
        h1{
          font-size: $font-size-base * 2;
          &+p{
            margin-bottom: 32px;
            font-size: $font-size-base + 2px;
          }
        }
        h2{
          font-size: $font-size-base + 15px;
          &+p{
            margin-bottom: 32px;
            font-size: $font-size-base + 2px;
          }
        }
        form{
          label{
            font-size: $font-size-base - 2px;
          }
          input{
            padding: 12px 16px;
            margin-bottom: 24px;
          }
          input[type="submit"]{
            padding-top: 10px;
            padding-bottom: 10px;
          }
          .registerBtn-wrapper{
            .linkToHomepage,
            .cancelBtn,
            input[type="submit"]{
              margin-bottom: 24px;
              padding-top: 10px;
              padding-bottom: 10px;
            }
          }
          .form-links{
            a{
              display: block;
              padding: 6px 8px;
              &:not(:last-child){
                border: none;
              }
            }
          }
          .showpass{
            right: 16px;
          }
          .CustomerPassword-wrapper,
          input,
          label{
            width: 100%;
          }
        }
      }
    }

    #RecoverPasswordForm{
      display: table;
    }
    #CustomerActivateAccountForm{
      @include media-query($small){
        form{
          .registerBtn-wrapper{
            flex-direction: column;
          }
        }
      }
    }

    .form-success{
      font: bold $font-size-base / normal $font-stack-body;
      text-align: center;
      margin: 32px 0 0;
    }

  }
}

.invited-answer{
  margin: 32px auto;
  display: table;
  padding: 16px;
  text-align: center;
  &:after{
    content: url('icon-invited-answer.svg');
