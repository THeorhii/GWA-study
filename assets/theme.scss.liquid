/*===============================================
*   Section Home Standards Style
* ================================================*/
.index-standards{
  display: grid;
  grid-template-rows: 32px 38px 1fr 38px 32px;
  grid-template-columns: 1fr 3.92fr 1.38fr 6.55fr;
  @media screen and (max-width: 650px){
    grid-template-rows: 1fr 1fr 32px 1fr 1fr;
    grid-template-columns: 1fr;
    &.page-width{
      padding: 0;
    }
  }
}
.index-standards__content{
  grid-row: 3 / 4;
  grid-column: 2 / 4;
  padding: 32px 56px;
  border-radius: 24px;
  -webkit-backdrop-filter: blur(30px);
  backdrop-filter: blur(30px);
  box-shadow: 0px 20px 30px #42424291;
  background-blend-mode: hard-light;
  background-image: linear-gradient(to bottom, rgba(245, 245, 245, 0.8), rgba(245, 245, 245, 0.8));
  ul{
    padding: 0;
  }
  li{
    font-family: $font-stack-header;
    font-size: 21px;
    font-weight: 500;
    line-height: 1.43;
    letter-spacing: 1px;
    color: #181818;
    margin-bottom: 16px;
    list-style: none;
    position: relative;
    padding-left: 28px;
    &::before{
      content: ' ';
      background: center / contain no-repeat url('{{ "icon-check.png" | asset_url }}');
      position: absolute;
      width: 22px;
      height: 30px;
      left: 0;
    }
  }
  .btn{
    margin: 30px auto 0;
    display: table;
    max-width: 368px;
    width: 100%;
    height: 56px;
    padding: 16px 0;
    border-radius: 4px;
    background-color: #fbb03a;
    font-family: $font-stack-body;
    font-size: $font-size-base;
    font-weight: bold;
    line-height: 1.5;
    letter-spacing: 1px;
    color: #010101;
    text-transform: uppercase;
  }
  @media screen and (max-width: 1050px){
    grid-column: 1 / 4;
    li{
      font-size: 18px;
      line-height: 1.2;
    }
  }
  @media screen and (max-width: 965px){
    padding: 24px 16px;
    li{
      font-size: 16px;
      line-height: 1;
      margin-bottom: 10px;
      padding-left: 20px;
      &::before{
        width: 18px;
        height: 18px;
      }
    }
    .btn{
      max-width: 212px;
      height: auto;
      padding-top: 6px;
      padding-bottom: 6px;
      font-size: 14px;
      margin-top: 24px;
    }
  }
  @media screen and (max-width: 650px){
    padding: 16px 8px;
    grid-row: 3 / 6;
    grid-column: 1 / 2;
    margin: 0 16px 24px;
  }
}
.index-standards__content-title{
  font-size: 36px;
  line-height: 1.33;
  margin: 0;
  font-family: $font-stack-header;
  font-weight: bold;
  letter-spacing: 1px;
  color: #010101;
  span{
    color: $color-bright;
  }
  p{
    margin: 0;
  }
  @media screen and (max-width: 965px){
    font-size: 28px;
    line-height: 1;
    text-align: center;
  }
}

.index-standards__image-wrap{
  grid-row: 2 / 5;
  grid-column: 3 / 5;
  border-radius: 24px;
  overflow: hidden;
  img{
    width: 100%;
    object-fit: cover;
    height: 100%;
  }
  @media screen and (max-width: 650px){
    grid-row: 1 / 4;
    grid-column: 1 / 2;
    border-radius: 0;
  }
}
