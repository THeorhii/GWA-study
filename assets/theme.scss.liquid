      input{
        margin-left: 0;
        margin-top: 16px;
      }
      button{
        margin: 0;
      }
    }
  }
}
@media screen and(max-width: 450px) {
  .customer-page {
    .customer-content {
      h2{
        font-size: 32px;
      }
    }
  .customer-full-address{
    flex-direction: column;
    a{
      margin-top: 16px;
    }
  }
  }
  .order-full-address-every{
    flex-direction: column;
    align-items: center;
  }
}


