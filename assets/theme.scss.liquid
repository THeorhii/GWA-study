.meal-packs-section{
  padding: 55px 16px 0;
  .meal-pack{
    position: relative;
    border: 1px solid transparent;
    border-radius: 0 24px 0 24px;
    margin-left: 16px;
    margin-right: 16px;
    margin-bottom: 56px;
    margin-top: 32px!important;
    text-transform: uppercase;
    transition: all 0.4s ease-in;

    img{
      height: fit-content;
      width: 100%;
      //height: 100%;
      border-radius: 0 24px 0 24px;
      object-fit: cover;
    }
    .meal-pack-title{
      position: absolute;
      top: 24px;
      left: 24px;
      margin: 0;
      font: bold 48px $font-stack-header;
      letter-spacing: 1px;
      color: $color-yellow;
      transition: all 0.4s ease-in;
    }
    .meal-pack-additional-info{
      position: absolute;
      bottom: 24px;
      left: 24px;
      right: 24px;
      /*display: flex;
      flex-direction: row;
      justify-content: space-between;*/
      letter-spacing: 1px;
      font: $font-size-base $font-stack-header;

      .meal-pack-subtitle{
        text-align: left;
        margin: 0;
        color: $color-white;
      }
      span{
        font-weight: bold;
        text-align: right;
        color: #F40009;
      }
    }
    .btn{
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      opacity: 0;
      transition: opacity 0.4s ease-in;
      padding: 17px 67px 16px;
      font-family: $font-stack-header;
      letter-spacing: 1px;
    }

    &::before{
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      opacity: 0;
      border-radius: 0 32px 0 32px;
      background: transparent;
      transition: opacity 0.4s ease-in;
    }

    &:hover{
      &::before{
        opacity: 0.8;
        background: url("hover-stripes.png");
        background-size: cover;
      }

      .meal-pack-title{
        mix-blend-mode: difference;
      }
      .btn{
        opacity: 1;
      }
    }
  }

  .slick-prev,
  .slick-next{
    width: 50px;
    height: 50px;
    background: #fbb03b80;
    border-radius: 6px;
    &:hover,
    &:focus{
      background: $color-bright;
    }
    &:before{
      content: url(icon-arrow-simple-right.svg);
    }
  }
  .slick-prev{
      transform: rotate(180deg);
      top: 44%;
      left: 10px;
      z-index: 10;
  }
  .slick-next{
      right: 10px;
    }
  .slick-track{
    .slick-center,

    .slick-current{
      border-color: rgb(251, 176, 59);
      filter: drop-shadow(1px 13px 13px rgba(251,176,59,0.7));
    }
  }
  .slick-dots{
    top: 0;
    bottom: auto;
    background: #f3f3f3;
    backdrop-filter: blur( 20px );
    -webkit-backdrop-filter: blur( 20px );

    li{
      //width: 8.75%;
      width: 12%;
      height: auto;
      margin: 0;
      padding: 0;
      font: bold $font-size-base + 2px $font-stack-help;
      letter-spacing: 1px;
      text-align: center;
      text-transform: uppercase;
      color: $color-product-text;

      &:not(:last-child){
        margin-right: 1.67%;
      }
      a{
        display: block;
        padding: 16px 0;
        color: #181818;
        &:hover{
          color: $color-bright;
        }
      }
    }

    .slick-active{
      color: $color-yellow;
      a{
        color: $color-bright;
      }
      &::after{
        content: '';
        position: absolute;
        bottom: 0;
        left: 50%;
        transform: translateX(-50%);
        width: 56px;
        border-bottom: 2px solid #fbb03b;
      }
    }
  }

  @include media-query($large-down) {
    .meal-pack{
      margin-left: 8px;
      margin-right: 8px;
      img{
        height: 100%;
      }
      .meal-pack-title{
        font-size: $font-size-base + 34px;
      }
      .meal-pack-additional-info{
        font-size: $font-size-base - 4px;
      }
    }
  }
  @include media-query($medium-down) {
    .meal-pack{
      .meal-pack-title{
        top: 16px;
        font-size: $font-size-base + 20px;
      }
      .meal-pack-additional-info{
        left: 16px;
        right: 16px;
        flex-direction: column;
        span{
          text-align: left;
        }
      }
    }

    .slick-dots{
      li{
        width: max-content;
        &:not(:last-child){
          margin-right: 3.5%;
        }
      }
    }
  }
  @include media-query($small){
    padding-right: 8px;
    padding-left: 8px;
    .slick-prev{
      top: 46%;
    }
    .meal-pack{
      margin-top: 72px;

      .meal-pack-title{
        left: 16px;
        font-size: $font-size-base * 2;
      }
      img{
        min-height: 360px;
      }
      .btn{
        padding: 9px 5% 8px;
      }
    }
    .slick-dots{
      li{
        font-size: $font-size-base;

        &:not(:last-child){
          margin-right: 31px;
        }
      }
    }
  }
  @include media-query($tablet){
    .meal-pack{
      margin-bottom: 32px;
    }
    .slick-dots{
      li{
        position: relative;
        &:not(:last-child){
          margin-right: 0;
        }
        &::before{
          content: "";
          position: absolute;
          bottom: 0;
          left: 0;
          right: 0;
        }
        a{
          padding: 16px;
        }
      }
    }
  }
  @media screen and(max-width: 784px){
    padding-top: 105px;
  }

  @include media-query($mobile){
    padding-top: 58px;
    .meal-pack{
      margin-right: 20px;
      margin-left: 20px;

      .meal-pack-title{
        top: 8px;
      }
      .meal-pack-additional-info{
        font-size: $font-size-base - 2px;
      }
    }
    .slick-dots{
      li{
        width: initial;
        line-height: 1.5;
        font-size: $font-size-base - 3px;
        &:not(:last-child){
          margin-right: 0;
        }
        a{
          padding-top: 8px;
          padding-bottom: 2px;
        }
      }
    }
  }

  @media screen and(max-width: 380px){
    padding-top: 88px;
  }
}
