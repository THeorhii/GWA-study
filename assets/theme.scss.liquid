.general-question{
  form{
    display: flex;
    flex-wrap: wrap;
    >div{
      width: 100%;
    }
  }
  label{
    margin-bottom: 32px;
  }
  .full-width{
    width: 100%;
  }
  .part-width{
    width: calc( 50% - 12px );
    &:last-child{
      margin-left: 24px;
    }
  }
  .general-question__sizes{
    display: flex;
    justify-content: space-between;
    label{
      max-width: 168px;
      &:not(:last-child){
        margin-right: 16px;
      }
    }
    span{
      position: absolute;
      bottom: 24px;
      right: 32px;
      font-family: $font-stack-body;
      font-size: 16px;
      text-transform: uppercase;
      letter-spacing: 1px;
      color: #181818;
