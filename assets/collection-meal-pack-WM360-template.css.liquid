.btn-answer-wm360 {
    position: relative;
    max-width: 368px;
    margin: 0 auto 16px;
}

@media only screen and (max-width: 480px) {
    .btn-answer-wm360 {
        max-width: 290px;
        margin-bottom: 24px;
    }
}

.btn-answer-wm360 input[type="radio"],
.btn-answer-wm360 input[type="checkbox"] {
    display: none;
}

.btn-answer-wm360 label {
    padding: 12px 16px 8px 52px;
    border-radius: 4px;
    border: solid 2px #e9e9e9;
    background-color: #FFFFFF;
    transition: all .3s;
    cursor: pointer;
    display: block;
    color: #868686;
    font-size: 18px;
    letter-spacing: 1px;
    font-family: futura-pt-condensed, Arial, sans-serif;
}

.btn-answer-wm360 input:checked ~ label {
    border-color: #010101;
    color: #010101;
}

@media only screen and (max-width: 480px) {
    .btn-answer-wm360 label {
        font-size: 16px;
        padding: 19px 16px 17px 52px;
    }
}

@media only screen and (max-width: 370px) {
    .btn-answer-wm360 label {
        font-size: 14px;
    }
}

.btn-answer-wm360__box {
    position: absolute;
    width: 24px;
    height: 24px;
    border: 2px solid #c4cdd5;
    transform: translateY(-50%);
    top: 50%;
    border-radius: 4px;
    left: 18px;
    transition: all .2s;
    pointer-events: none;
}

.btn-answer-wm360__box::after {
    content: '\2714';
    display: block;
    position: absolute;
    font-size: 22px;
    border-radius: 50%;
    transform: translate(-50%, -50%);
    top: 50%;
    left: 50%;
    opacity: 0;
    transition: all .2s;
}

input:checked ~ .btn-answer-wm360__box {
    border-color: #010101;
    background-color: #fbb03c;
}

input:checked ~ .btn-answer-wm360__box::after {
    opacity: 1;
}

.WM360-quiz__block-btn-back {
    position: absolute;
    bottom: -50px;
    transform: translate(-50%, 0);
    left: 50%;
    font-family: var(--font-stack-header);

}

.WM360-quiz__block-btn-back svg {
    transform: rotate(90deg);
    width: 18px;
    height: 12px;
}

.btn-link {
    border: none;
    background: none;
    text-decoration: none;
    text-transform: uppercase;
}

.link-grey {
    color: #868686;
    transition: .3s;
}

.link-grey:hover {
    color: #000000;
}

.link-grey svg,
.link-grey path {
    fill: #868686;
    transition: .3s;
}

.link-grey:hover svg,
.link-grey:hover path {
    fill: #000000;
}

.btn-black {
    border: 2px solid #010101;
    color: #fbb03b;
    background-color: #010101;
    transition: .3s;
}

.btn-black:hover {
    color: #010101;
    background-color: #fbb03b;
}

.btn-medium {
    padding: 17px 68px 16px;
    font-family: futura-pt, Arial, sans-serif;
