/*<========================================================== WM360 standards =================================================>*/
.wm360-section-standards {
    padding: 64px 0 32px;
}
.wm360-standards__title--main {
    text-align: center;
    font-family: var(--font-stack-header);
    font-size: 48px;
    font-weight: bold;
    font-stretch: normal;
    font-style: normal;
    line-height: normal;
    letter-spacing: 1px;
    text-align: center;
    color: var(--color-dark-text);
    text-transform: uppercase;
}

.wm360-standards {
    display: grid;
    grid-template-rows: 32px 38px 1fr 38px 32px;
    grid-template-columns: 1fr 3.92fr 1.6fr 4fr;
}

.wm360-standards__content {
    grid-row: 3 / 4;
    grid-column: 3 / 5;
    padding: 32px 48px;
    border-radius: 24px;
    -webkit-backdrop-filter: blur(30px);
    backdrop-filter: blur(30px);
    box-shadow: 0px 20px 30px #42424291;
    background-blend-mode: hard-light;
    background-image: linear-gradient(to bottom, rgba(245, 245, 245, 0.8), rgba(245, 245, 245, 0.8));
}
.wm360-standards__content ul {
    padding: 0;
}
.wm360-standards__content li {
    font-family: var(--font-stack-header);
    font-size: 21px;
    font-weight: 500;
    line-height: 1.43;
    letter-spacing: 1px;
    color: var(--color-text-black);
    margin-bottom: 16px;
    list-style: none;
    position: relative;
    padding-left: 28px;
}
.wm360-standards__content li::before {
    content: ' ';
    background: center / contain no-repeat url('{{ "icon-check--red.svg" | asset_url }}');
    position: absolute;
    width: 22px;
    height: 30px;
    left: 0;
}
.wm360-standards__content .btn {
    margin: 30px auto 0;
    display: table;
    max-width: 368px;
    width: 100%;
    height: 56px;
    padding: 16px 0;
    border-radius: 4px;
    background-color: var(--color-yellow);
    font-family: var(--font-stack-header);
    font-size: var(--font-size-base);
    font-weight: bold;
    line-height: 1.5;
    letter-spacing: 1px;
    color: var(--color-dark-text);
    text-transform: uppercase;
}

.wm360-standards__content-title {
    font-size: 36px;
    line-height: 1.33;
    margin: 0;
    font-family: var(--font-stack-header);
    font-weight: bold;
    letter-spacing: 1px;
    color: var(--color-dark-text);
    text-transform: none;
}
.wm360-standards__content-title span {
    color: var(--color-bright);
}
.wm360-standards__content-title p {
    margin: 0;
}

.wm360-standards__image-wrap {
    grid-row: 2 / 5;
    grid-column: 1 / 4;

    border-radius: 24px;
    overflow: hidden;
}
.wm360-standards__image-wrap img {
    width: 100%;
    object-fit: cover;
    height: 100%;
}

.wm360-standards__content-text {
    font: 16px var(--font-stack-body);
    line-height: 1.5;
    letter-spacing: normal;
    color: var(--color-light-grey);
}


@media screen and (max-width: 1050px) {

    .wm360-standards__content li {
        font-size: 18px;
        line-height: 1.2;
}
    }
@media screen and (max-width: 965px) {
    .wm360-standards__content {
        padding: 24px 16px;
    }
    .wm360-standards__content li {
        font-size: 21px;
        line-height: 32px;
        margin-bottom: 10px;
        padding-left: 20px;
    }
    .wm360-standards__content li::before {
        width: 18px;
        height: 18px;
        top: 7px;
    }
    .wm360-standards__content .btn {
        max-width: 212px;
        height: auto;
        padding-top: 6px;
        padding-bottom: 6px;
        margin-top: 24px;
    }
    .wm360-standards__content-title {
        font-size: 36px;
        line-height: 1;
        letter-spacing: 1px;
        text-align: center;
    }
    .wm360-standards__content-text {
        text-align: center;
    }
}

@media (max-width: 880px) {
    .wm360-section-standards {
        padding: 56px 0 64px;
    }
    .wm360-standards {
        display: grid;
        grid-template-rows: 1fr;
        grid-template-columns: 1fr 1fr 1fr 1fr 1fr;
        align-items: center;
        padding: 0 40px;
    }
    .wm360-standards__image-wrap {
        grid-row: 1;
        grid-column: 1/6;
        border-radius: 24px;
        overflow: hidden;
    }
    .wm360-standards__content {
        grid-row: 2/3;
        grid-column: 1/6;
        width: 90%;
        margin: 0 auto;
        margin-top: -50px;
    }
    .wm360-standards__content li::before {
        top:0;
    }
    .wm360-standards__content li {
        line-height: 21px;
        margin-bottom: 15px;
    }

    .wm360-standards__title--main {
        margin-bottom: 32px;
    }
}

@media screen and (max-width: 480px) {
    .wm360-section-standards {
        padding: 24px 0 48px;

    }
    .wm360-standards.page-width {
        padding: 0;
    }
    .wm360-standards__image-wrap {
        border-radius: 0;
    }
    .wm360-standards__title--main {
        font-size: 28px;
        margin-bottom: 14px;
    }
    .wm360-standards__content-text {
        font-size:14px;
    }
    .wm360-standards__content li {
        font-size: 14px;
    }
    .wm360-standards__content-title {
        font-size: 28px;
    }
    .wm360-standards__content li {
        margin-bottom: 0;
    }
}

