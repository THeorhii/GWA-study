{% assign mealplanMain = product.metafields.mealplanDetails.mealplanDetails %}
{% assign count_days = mealplanMain.mealplan_days %}
{% assign mealpackDiscount = mealplanMain.mealplan_discount | times: 1 %}
{% assign mealPackIsFixed = false %}
{% if mealplanMain.mealplan_type == 'fixed' %}
    {% assign mealPackIsFixed = true %}
{% endif %}

<div class="days-meal-plan{% if mealpackDiscount > 0 %} meal-pack-has-discount{% endif %}{% if mealPackIsFixed %} meal-pack-fixed{% endif %}"
     data-discount-type="{{ mealplanMain.mealplan_discount_type }}"
     data-discount="{% if mealplanMain.mealplan_discount_type == 'FIXED' %}{{ mealpackDiscount | times: 100 }}{% else %}{{ mealpackDiscount }}{% endif %}">
    {%- unless mealPackIsFixed -%}
        {%- for i in (1..count_days) -%}
            {% capture count_day %}mealplansettings-{{ i }}{% endcapture %}
            <div class="daily-meal-plan" data-day="{{ i }}" data-title="{{ product.title }}" data-id="{{ product.id }}">
                {% assign mealplanMain = product.metafields.mealplanDetails.mealplanDetails %}
                {% assign mealplanPlan = product.metafields.mealplansettings[count_day] %}
                {% assign mealplanSize = product.metafields.mealplansettings[count_day].size %}

                {% assign dayProtein = mealplanPlan[0].protein | times: 1 %}
                {% assign dayFat = mealplanPlan[0].fat | times: 1 %}
                {% assign dayCarbs = mealplanPlan[0].carbs | times: 1 %}
                {% assign dayCalories = mealplanPlan[0].calories | times: 1 %}
                {%- for i in (1..mealplanSize) -%}
                    {% assign protein = mealplanPlan[i].protein | times: 1 %}
                    {% assign dayProtein = dayProtein | plus: protein %}
                    {% assign fat = mealplanPlan[i].fat | times: 1 %}
                    {% assign dayFat = dayFat | plus: fat %}
                    {% assign carbs = mealplanPlan[i].carbs | times: 1 %}
                    {% assign dayCarbs = dayCarbs | plus: carbs %}
                    {% assign calories = mealplanPlan[i].calories | times: 1 %}
                    {% assign dayCalories = dayCalories | plus: calories %}
                {%- endfor -%}

                <h4>Day {{ i }}</h4>

                <div class="day-nutrition">
                    <p>PROTEIN: <span><span class="day-protein">{{ dayProtein }}</span>G</span></p>
                    <p>FAT: <span><span class="day-fat">{{ dayFat }}</span>G</span></p>
                    <p>CARBS: <span><span class="day-carbs">{{ dayCarbs }}</span>G</span></p>
                    <p>CAL: <span class="day-cal">{{ dayCalories }}</span></p>
                </div>

                <div class="package-plan">
                    {%- assign sortedMealplanPlan = mealplanPlan | sort: "mealSortOrder" -%}
                    {%- for i in (0..mealplanSize) -%}
                        {% if sortedMealplanPlan[i] %}
                            {% assign product_key = sortedMealplanPlan[i].handle %}
                            {%- paginate collections['all'].products by 500 -%}
                                {%- for product in collections['all'].products -%}
                                    {% if product.handle == product_key %}
                                        {% assign meal_product = product %}
                                        <div class="meal" data-variant="{{ sortedMealplanPlan[i].variantId }}"
                                             data-variant-price="{{ sortedMealplanPlan[i].price }}"
                                             data-variant-price_discount="{{ sortedMealplanPlan[i].discountedPrice }}"
                                             data-special="{%- for tag in meal_product.tags -%}{%- if tag == 'wm_clean_vegan' -%}wm_clean_vegan{%- endif -%}{%- endfor -%}">
                                            <div class="meal-image">
                                                <img srcset="{{ sortedMealplanPlan[i].imgSmall | replace: '.jpg', '_250x.jpg' }},
                                             {{ sortedMealplanPlan[i].imgSmall | replace: '.jpg', '_375x.jpg' }} 1.5x,
                                             {{ sortedMealplanPlan[i].imgSmall | replace: '.jpg', '_500x.jpg' }} 2x"
                                                     src="{{ sortedMealplanPlan[i].imgSmall | replace: '.jpg', '_300x.jpg' }}"
                                                     alt="{{ sortedMealplanPlan[i].title }}">
                                            </div>
                                            <div class="meal-content">
                                                <p class="meal-number">meal {{ i | plus: 1 }}</p>
                                                <p class="meal-title">{{ sortedMealplanPlan[i].title }}</p>
                                                <div class="meal-nutrition">
                                                    <p>
                                                        <span class="to-day-protein">{{ sortedMealplanPlan[i].protein }}</span>Protein
                                                        (g)</p>
                                                    <p>
                                                        <span class="to-day-cal">{{ sortedMealplanPlan[i].calories }}</span>Calories
                                                    </p>
                                                    <p>
                                                        <span class="to-day-carbs">{{ sortedMealplanPlan[i].carbs }}</span>Carbs
                                                        (g)</p>
                                                    <p><span class="to-day-fat">{{ sortedMealplanPlan[i].fat }}</span>Fat
                                                        (g)</p>
                                                </div>
                                            </div>
                                            <div class="meal-event">
                                                <button class="change-meal"
                                                        data-find-calories="{{ sortedMealplanPlan[i].calories }}"
                                                        data-meal-number="{{ i | plus: 1 }}">
                                                    <span>SWAP</span> {% include 'icon-edit-pen' %}</button>

                                                <button class="delete-meal hide">{% include 'icon-circle-delete' %}</button>
                                            </div>
                                        </div>
                                    {% endif %}
                                {%- endfor -%}
                            {%- endpaginate -%}
                        {% endif %}
                    {%- endfor -%}
                    {% comment %}
                        {% if mealplanMain.mealplan_breakfast == true %}
                            <div class="meal empty-meal" data-variant="">
                                <img src="{{ mealplanPlan[0].imgSmall }}">
                                <p class="num-meal">BREAKFAST</p>
                            </div>
                        {% endif %}
                        {% if mealplanMain.mealplan_snacks == true %}
                            <div class="meal empty-meal" data-variant="">
                                <img src="{{ mealplanPlan[0].imgSmall }}">
                                <p class="num-meal">SNACKS</p>
                            </div>
                        {% endif %}
