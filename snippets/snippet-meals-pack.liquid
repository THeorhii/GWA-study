{% assign r_price = mealplanMain.mealplan_regular_price | times: 100 %}
{% assign d_price = mealplanMain.mealplan_price | times: 100 %}
<div class="block-add-meal-pack{% if r_price > d_price %} pack-with-discount{% endif %}">
    <div>
        <div class="bottom-fixed-logo">
            {% include 'icon-wm-logo-black-text' %}
        </div>
        <p>
            {% include 'icon-bottom-truck' %}
            <span>free delivery if you subscribe</span>
        </p>
        <button class="add-to-cart-plan" data-type-discount="{{ mealplanMain.mealplan_discount_type }}"
                data-add-meal-pack-to-cart
                data-show-discount="{{ mealpackDiscount }}"
                onclick="pintrk('track', 'addtocart', {value: 100, order_quantity: 1, currency: 'USD'});">

            $ {% if r_price > d_price %}
                <span class="old-pack-sum">{{ mealplanMain.mealplan_regular_price | times: 100 | money_without_currency }}</span>
            {% endif %}
            <span class="new-pack-sum">{{ mealplanMain.mealplan_price | times: 100 | money_without_currency }}</span>

            | Add to cart
        </button>
    </div>
</div>

