<script>
    /* wm360 FAQS */
    $('.wm360-faqs-block button').click(function () {
        let faq_block = $(this).parent();
        if(faq_block.hasClass('active')){
            faq_block.find('.wm360-faqs-block__answer').fadeOut(400);
            faq_block.removeClass('active');
        }else{
            faq_block.find('.wm360-faqs-block__answer').fadeIn(400);
            faq_block.addClass('active');
        }
    });

</script>